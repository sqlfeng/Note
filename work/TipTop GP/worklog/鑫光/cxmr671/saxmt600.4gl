#FUNCTION t600_upd_oga50()             #CHI-CB0008 mark
FUNCTION t600_upd_oga50(p_chkwind)     #CHI-CB0008 add
  DEFINE p_chkwind     LIKE type_file.chr1   #CHI-CB0008 add
  DEFINE l_cnt         LIKE type_file.num5
  DEFINE l_ogb03       LIKE ogb_file.ogb03
  DEFINE l_ogb13       LIKE ogb_file.ogb13
  DEFINE l_ogb917      LIKE ogb_file.ogb917
  DEFINE l_ogb14       LIKE ogb_file.ogb14
  DEFINE l_ogb14t      LIKE ogb_file.ogb14t
  DEFINE l_ogb31       LIKE ogb_file.ogb31
  DEFINE l_oea21       LIKE oea_file.oea21
  #FUN-C10040--------add------str-------
  DEFINE l_rtz04       LIKE rtz_file.rtz04
  DEFINE l_rtz06       LIKE rtz_file.rtz06
  DEFINE l_chk_gec07   LIKE type_file.chr4
  DEFINE l_oga213      LIKE gec_file.gec07
  DEFINE l_ogi07       LIKE ogi_file.ogi07
  DEFINE l_n           LIKE type_file.num5
  #FUN-C10040--------add------end-------
  DEFINE l_conu        LIKE type_file.chr1   #CHI-CB0008 add

   SELECT COUNT(*) INTO l_cnt from ogb_file
    WHERE ogb01 = g_oga.oga01
   IF l_cnt > 0 THEN
      #FUN-C10040--------add------str-------
      IF g_azw.azw04 = '2' THEN
         #取該營運中心在 arti200(門店基本資料維護作業) 中的 rtz06(散客代碼)
         SELECT rtz04, rtz06 INTO l_rtz04,l_rtz06
           FROM rtz_file
          WHERE rtz01 = g_oga.ogaplant
      END IF
      LET l_chk_gec07 = 'Y'
      IF g_azw.azw04 = '2' AND NOT cl_null(l_rtz04) THEN
         LET l_chk_gec07 = 'N'
      END IF
      IF l_chk_gec07 = 'Y' THEN
      #FUN-C10040--------add------end-------
         ##稅別改變，是否跟新單身金額      #CHI-CB0008 mark
         #IF cl_confirm('axm-936') THEN    #CHI-CB0008 mark
         #CHI-CB0008 add begin---
         LET l_conu = NULL
         IF p_chkwind = 'N' THEN
            LET l_conu = 'Y'
         END IF
         IF p_chkwind = 'Y' THEN
            IF cl_confirm('axm-936') THEN
               LET l_conu = 'Y'
            END IF
         END IF
         IF l_conu = 'Y' THEN
         #CHI-CB0008 add end-----
            DECLARE oga21_cs CURSOR FOR
             SELECT ogb03,ogb13,ogb917,ogb31 FROM ogb_file
              WHERE ogb01 = g_oga.oga01
            FOREACH oga21_cs INTO l_ogb03,l_ogb13,l_ogb917,l_ogb31
               #先判斷單身的訂單號是否為空，不為空的話，再看改后稅率是否相同
               IF NOT cl_null(l_ogb31) THEN
                  SELECT oea21 INTO l_oea21 FROM oea_file
                   WHERE oea01 = l_ogb31
                  IF g_oga.oga21 != l_oea21 THEN
                     CALL cl_err('sel oea','axm-142',0)
                     RETURN FALSE
                  END IF
               END IF
               #FUN-C10040 ----add---begin ---
               IF g_azw.azw04 = '2' AND NOT s_industry("slk") THEN  #TQC-C20418 ADD not slk
                  CALL t620_sub(g_ogb[l_ac].ogb04,l_rtz04,g_oga.oga213,l_ogb917,l_ogb13,t_azi04)
                       RETURNING l_ogb14,l_ogb14t
               ELSE
               #FUN-C10040 ----add---end -----
                  IF g_oga.oga213 = 'N' THEN
                     LET l_ogb14 =l_ogb917*l_ogb13
                     LET l_ogb14t=l_ogb14*(1+g_oga.oga211/100)
                     #CALL cl_digcut(l_ogb14,g_azi04) RETURNING l_ogb14   #MOD-C30821 mark
                     #CALL cl_digcut(l_ogb14t,g_azi04) RETURNING l_ogb14t #MOD-C30821 mark
                     CALL cl_digcut(l_ogb14,t_azi04) RETURNING l_ogb14    #MOD-C30821 add
                     CALL cl_digcut(l_ogb14t,t_azi04) RETURNING l_ogb14t  #MOD-C30821 add
                  ELSE
                     LET l_ogb14t=l_ogb917*l_ogb13
                     LET l_ogb14 =l_ogb14t/(1+g_oga.oga211/100)
                     #CALL cl_digcut(l_ogb14t,g_azi04) RETURNING l_ogb14t #MOD-C30821 mark
                     #CALL cl_digcut(l_ogb14,g_azi04) RETURNING l_ogb14   #MOD-C30821 mark
                     CALL cl_digcut(l_ogb14t,t_azi04) RETURNING l_ogb14t  #MOD-C30821 add
                     CALL cl_digcut(l_ogb14,t_azi04) RETURNING l_ogb14    #MOD-C30821 add
                  END IF
               END IF #FUN-C10040 add
               UPDATE ogb_file SET ogb14 = l_ogb14,
                                   ogb14t= l_ogb14t
                WHERE ogb01 = g_oga.oga01 AND ogb03 = l_ogb03
               IF SQLCA.SQLCODE THEN
                  CALL cl_err3("upd","ogb_file",g_oga.oga01,l_ogb03,STATUS,"","upd ogb14,ogb14t:",1)
                  RETURN FALSE
                  EXIT FOREACH
               END IF
            END FOREACH
           #CALL t600_b_fill(' 1=1')  #但是不顯示金額，所以不需要

           #FUN-A20022 ADD----------UPDATE THE POINT---------------------------
             IF NOT cl_null(g_oga.oga87) THEN
                LET g_oga.oga95= t600_oga95_amount()
                IF cl_null(g_oga.oga95) THEN LET g_oga.oga95=0 END IF #MOD-B10154
                LET g_oga.oga95 = cl_digcut(g_oga.oga95,g_rcj19)   #FUN-D80037
                UPDATE oga_file SET oga95 = g_oga.oga95
                              WHERE oga01 = g_oga.oga01
                                AND oga87 = g_oga.oga87
                IF SQLCA.sqlcode OR SQLCA.sqlerrd[3] = 0 THEN
                   CALL cl_err3("upd","oga_file",g_oga.oga01,"",SQLCA.sqlcode,"","",1)
                END IF
                DISPLAY BY NAME g_oga.oga95
             END IF
           #FUN-A20022 END----------------------------------------------
            SELECT SUM(ogb14) INTO g_oga.oga50 FROM ogb_file
             WHERE ogb01 = g_oga.oga01
            CALL cl_digcut(g_oga.oga50,g_azi04) RETURNING g_oga.oga50
            IF cl_null(g_oga.oga50) THEN LET g_oga.oga50 = 0 END IF
         END IF
      END IF #FUN-C10040 add
   END IF
   RETURN TRUE
END FUNCTION

