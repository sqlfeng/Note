FUNCTION t600_cancel_price()
DEFINE l_i    LIKE type_file.num5
DEFINE l_ogb  RECORD LIKE ogb_file.*      #MOD-C30244 add
DEFINE l_rtz04 LIKE rtz_file.rtz04        #MOD-C30244 add
DEFINE l_n    LIKE type_file.num5         #MOD-C30244 add

   IF s_shut(0) THEN RETURN END IF
   IF g_oga.oga01 IS NULL THEN RETURN END IF
   IF g_oga.ogaconf = 'Y' THEN CALL cl_err('',9023,0) RETURN END IF
   IF g_oga.ogaconf = 'X' THEN CALL cl_err('',9024,0) RETURN END IF
#MOD-C30244 ----------------------STA
   SELECT COUNT(*) INTO l_n  FROM rxy_file WHERE rxy00 = '02' AND rxy01 = g_oga.oga01
   IF l_n > 0 THEN
      CALL cl_err('','axm-722',0)
      RETURN
   END IF
#MOD-C30244 ----------------------END

   OPEN WINDOW t600_s_w AT p_row,p_col WITH FORM "axm/42f/axmt410_s"
      ATTRIBUTE (STYLE = g_win_style CLIPPED)

   CALL cl_ui_init()
   CALL t600_s_fill()

   INPUT ARRAY g_oeb7 WITHOUT DEFAULTS FROM s_oeb7.*
      ATTRIBUTE(COUNT=g_rec_b,MAXCOUNT=g_max_rec,UNBUFFERED,
                INSERT ROW=FALSE,DELETE ROW=FALSE,
                APPEND ROW=FALSE)
      ON ACTION all
         FOR l_i = 1 TO g_oeb7.getLength()
            LET g_oeb7[l_i].status = 'Y'
         END FOR

      ON ACTION no_all
         FOR l_i = 1 TO g_oeb7.getLength()
            LET g_oeb7[l_i].status = 'N'
         END FOR
   END INPUT

   IF INT_FLAG THEN
      CALL g_oeb7.clear()
      LET INT_FLAG = 0
      CLOSE WINDOW t600_s_w
      RETURN
   END IF


   BEGIN WORK
   FOR l_i = 1 TO g_oeb7.getLength()
       IF g_oeb7[l_i].status = 'Y' THEN

#MOD-C30244 ---------------------STA
#         UPDATE ogb_file SET ogb47 = 0
#          WHERE ogb01 = g_oga.oga01 AND ogb03 = g_oeb7[l_i].oeb03
#         IF SQLCA.sqlcode OR SQLCA.sqlerrd[3] = 0 THEN
#           #ROLLBACK WORK        #FUN-B60011 程式搬移
#            CALL cl_err3("upd","ogb_file",g_oga.oga01,"",SQLCA.sqlcode,"","",1)
#            ROLLBACK WORK        #FUN-B60011 程式搬移
#            EXIT FOR
#         END IF

          SELECT * INTO l_ogb.*  FROM ogb_file WHERE ogb01 = g_oga.oga01 AND ogb03 = g_oeb7[l_i].oeb03
          SELECT azi03,azi04 INTO t_azi03,t_azi04
            FROM azi_file
           WHERE azi01=g_oga.oga23

          SELECT rtz04 INTO l_rtz04 FROM rtz_file WHERE rtz01 = g_oga.ogaplant

          CALL t620_sub(l_ogb.ogb04,l_rtz04,g_oga.oga213,l_ogb.ogb917,l_ogb.ogb37,t_azi04)
                 RETURNING l_ogb.ogb14,l_ogb.ogb14t


          UPDATE ogb_file SET ogb47 = 0,
                              ogb13 = ogb37,
                              ogb14 = l_ogb.ogb14,
                              ogb14t = l_ogb.ogb14t
           WHERE ogb01 = g_oga.oga01 AND ogb03 = g_oeb7[l_i].oeb03
          IF SQLCA.sqlcode OR SQLCA.sqlerrd[3] = 0 THEN
             CALL cl_err3("upd","ogb_file",g_oga.oga01,"",SQLCA.sqlcode,"","",1)
             ROLLBACK WORK
             EXIT FOR
          END IF

          DELETE FROM ogi_file WHERE ogi01 = g_oga.oga01 AND ogi02 = g_oeb7[l_i].oeb03
          IF SQLCA.sqlcode THEN
             CALL cl_err3("del","ogi_file",g_oga.oga01, g_oeb7[l_i].oeb03,SQLCA.sqlcode,"","",1)
             ROLLBACK WORK
             EXIT FOR
          END IF

          call t620_ins_ogi(l_ogb.ogb03,l_ogb.ogb04,l_ogb.ogb917,l_ogb.ogb37)
          IF g_success = 'N' THEN
             ROLLBACK WORK
             LET g_success = 'Y'
             EXIT FOR
          END IF

          UPDATE rxc_file SET rxc06 = 0
           WHERE rxc00 = '02' AND rxc01 = g_oga.oga01 AND rxc02 = g_oeb7[l_i].oeb03
          IF SQLCA.sqlcode OR SQLCA.sqlerrd[3] = 0 THEN
             CALL cl_err3("upd","ogb_file",g_oga.oga01,"",SQLCA.sqlcode,"","",1)
             ROLLBACK WORK
             EXIT FOR
          END IF

          IF g_aza.aza50='Y' THEN
             CALL t600_oga50_sum()
          ELSE
             CALL t600_bu()
          END IF


#MOD-C30244 ---------------------END
       END IF
   END FOR

  #TQC-CA0017 add start---
   IF NOT cl_null(g_oga.oga87) THEN
      LET g_oga.oga95= t600_oga95_amount()
      IF cl_null(g_oga.oga95) THEN LET g_oga.oga95=0 END IF
      LET g_oga.oga95 = cl_digcut(g_oga.oga95,g_rcj19)   #FUN-D80037
      UPDATE oga_file SET oga95 = g_oga.oga95
                    WHERE oga01 = g_oga.oga01
                      AND oga87 = g_oga.oga87
      IF SQLCA.sqlcode OR SQLCA.sqlerrd[3] = 0 THEN
         CALL cl_err3("upd","oga_file",g_oga.oga01,"",SQLCA.sqlcode,"","",1)
      END IF
   END IF
  #TQC-CA0017 add end---

   COMMIT WORK
   CLOSE WINDOW t600_s_w
   DISPLAY BY NAME g_oga.oga95           #TQC-CA0017 add
   CALL t600_b1_fill(" 1=1")
