FUNCTION t600_chk_ogb31_last()
   DEFINE l_sql       STRING
   DEFINE li_result   LIKE type_file.num5
   DEFINE l_oea99_1     LIKE oea_file.oea99
   DEFINE l_oea99     LIKE oea_file.oea99
   DEFINE l_oea21_1     LIKE oea_file.oea21
   DEFINE l_oea21     LIKE oea_file.oea21
   DEFINE l_oea23_1     LIKE oea_file.oea23
   DEFINE l_oea23     LIKE oea_file.oea23
   DEFINE l_oea32_1     LIKE oea_file.oea32
   DEFINE l_oea32     LIKE oea_file.oea32
   DEFINE l_poz       RECORD LIKE poz_file.*
   DEFINE l_ogb31     LIKE ogb_file.ogb31
   DEFINE l_flow      LIKE oea_file.oea904
   DEFINE l_poy04     LIKE poy_file.poy04
   DEFINE l_azp03     LIKE azp_file.azp03
   DEFINE p_plant_new LIKE azp_file.azp01
   DEFINE l_poy02_last LIKE poy_file.poy02 #最終站
   DEFINE l_poy02      LIKE poy_file.poy02 #目前站別

   SELECT MAX(poy02) INTO l_poy02_last FROM poy_file WHERE poy01 = g_flow
   SELECT poy02 INTO l_poy02 FROM poy_file WHERE poy01 = g_flow AND poy04 = g_plant
   LET l_ogb31 = g_ogb[1].ogb31
   IF g_ogb[l_ac].ogb31 <> l_ogb31 THEN
      IF g_oga.oga909 = 'Y' AND (g_oga.oga09 = '4' OR g_oga.oga09 = '5')
         AND g_poz.poz00 = '1' AND g_poz.poz011 = '2' AND g_poz.poz09 = 'Y' AND l_poy02_last = l_poy02 THEN

         CALL t600sub_chkpoz(g_oga.*,NULL) RETURNING li_result,l_poz.*,l_oea99_1,l_flow
         IF li_result THEN
            SELECT poy04 INTO l_poy04  #營運中心
              FROM poy_file
             WHERE poy01 = l_poz.poz01
               AND poy02 = '0'   #起始站
            SELECT azp03 INTO l_azp03 FROM azp_file WHERE azp01 = l_poy04

            LET g_plant_new = l_poy04
            LET p_plant_new = g_plant_new

            LET l_azp03 = s_dbstring(l_azp03 CLIPPED)
            #取項次一訂單資料
            LET l_sql = " SELECT oea21,oea23,oea32 FROM ",cl_get_target_table(p_plant_new,'oea_file'),
                        "  WHERE oea99 = '",l_oea99_1,"'"
            CALL cl_replace_sqldb(l_sql) RETURNING l_sql
            CALL cl_parse_qry_sql(l_sql,p_plant_new) RETURNING l_sql
            PREPARE oea21_pre2 FROM l_sql
            DECLARE oea21_cs2 CURSOR FOR oea21_pre2
            OPEN oea21_cs2
            IF STATUS THEN
               CALL cl_err('open oea21 oea23 oea32',STATUS,1)
            END IF
            FETCH oea21_cs2 INTO l_oea21_1,l_oea23_1,l_oea32_1
            IF STATUS THEN
               CALL cl_err('fetch oea21 oea23 oea32',STATUS,1)
            END IF
            #其他項次資料
            SELECT oea99 INTO l_oea99 FROM oea_file WHERE oea01 = g_ogb[l_ac].ogb31
            LET l_sql = " SELECT oea21,oea23,oea32 FROM ",cl_get_target_table(p_plant_new,'oea_file'),
                        "  WHERE oea99 = '",l_oea99,"'"
            CALL cl_replace_sqldb(l_sql) RETURNING l_sql
            CALL cl_parse_qry_sql(l_sql,p_plant_new) RETURNING l_sql
            PREPARE oea21_pre3 FROM l_sql
            DECLARE oea21_cs3 CURSOR FOR oea21_pre3
            OPEN oea21_cs3
            IF STATUS THEN
               CALL cl_err('open oea21 oea23 oea32',STATUS,1)
            END IF
            FETCH oea21_cs3 INTO l_oea21,l_oea23,l_oea32
            IF STATUS THEN
               CALL cl_err('fetch oea21 oea23 oea32',STATUS,1)
            END IF
         END IF
         IF l_oea21_1 != l_oea21 THEN    #稅別不符
            CALL cl_err('sel oea','axm-977',0) RETURN FALSE
            RETURN FALSE
         END IF
         IF l_oea23_1 != l_oea23 THEN    #幣別不符
            CALL cl_err('sel oea','axm-984',0) RETURN FALSE
            RETURN FALSE
         END IF
         IF l_oea32_1 != l_oea32 THEN    #收款條件不符
            CALL cl_err('sel oea','axm-978',0) RETURN FALSE
            RETURN FALSE
         END IF
      END IF
   END IF
   RETURN TRUE
END FUNCTION
