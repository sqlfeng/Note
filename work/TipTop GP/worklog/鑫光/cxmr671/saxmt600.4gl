FUNCTION t600_chk_ogb66()
DEFINE l_oga      RECORD LIKE oga_file.*
   SELECT * INTO l_oga.* FROM oga_file WHERE oga01=g_ogb[l_ac].ogb66
   IF l_oga.oga09 = '4' THEN                       #分批簽收功能,需控卡輸入[三角貿易出貨單]
      CALL cl_err(g_ogb[l_ac].ogb66,'axm1208',0)
      RETURN FALSE
   END IF
   IF l_oga.ogaconf != 'Y' THEN                    #原單據尚未確認, 不可再進行其它異動處理!
      CALL cl_err(g_ogb[l_ac].ogb66,'axm-184',0)
      RETURN FALSE
   END IF
   IF l_oga.oga65 <> 'Y' THEN
      CALL cl_err(g_ogb[l_ac].ogb66,'axm-720',0)   #此單據未做客戶出貨簽收!
      RETURN FALSE
   END IF
   IF l_oga.ogapost <> 'Y' THEN
      CALL cl_err(g_ogb[l_ac].ogb66,'axm-206',0)   #此出貨單尚未扣帳!
      RETURN FALSE
   END IF
   IF l_oga.oga00 <> g_oga.oga00 OR cl_null(l_oga.oga00) THEN
      CALL cl_err(g_ogb[l_ac].ogb66,'axm-147',0)   #簽收單與出貨單的出貨類別不同,請重新輸入!
      RETURN FALSE
   END IF
   IF l_oga.oga08 <> g_oga.oga08 THEN              #出貨單合併簽收,國內外出貨型態需一致
      CALL cl_err(g_ogb[l_ac].ogb66,'axm1202',0)
      RETURN FALSE
   END IF
   IF l_oga.oga03 <> g_oga.oga03 THEN              #出貨單合併簽收,帳款客戶需一致
      CALL cl_err(g_ogb[l_ac].ogb66,'axm1203',0)
      RETURN FALSE
   END IF
   IF l_oga.oga04 <> g_oga.oga04 THEN
      CALL cl_err(g_ogb[l_ac].ogb66,'axm1204',0)   #出貨單合併簽收,收貨客戶需一致
      RETURN FALSE
   END IF
   IF l_oga.oga21 <> g_oga.oga21 THEN              #稅別不符
      CALL cl_err(g_ogb[l_ac].ogb66,'axm-142',0)
      RETURN FALSE
   END IF
   IF l_oga.oga23 <> g_oga.oga23 THEN              #幣別不符
      CALL cl_err(g_ogb[l_ac].ogb66,'axm-144',0)
      RETURN FALSE
   END IF
   IF l_oga.oga32 <> g_oga.oga32 THEN
      CALL cl_err(g_ogb[l_ac].ogb66,'axm-143',0)   #收款條件不符!
      RETURN FALSE
   END IF
   RETURN TRUE
END FUNCTION

FUNCTION t600_chk_ogb67()

   IF NOT cl_null(g_ogb[l_ac].ogb67) THEN
      IF g_ogb[l_ac].ogb67 != g_ogb_t.ogb67 AND g_ogb_t.ogb17='Y' THEN
         LET g_ogb[l_ac].ogb67 = g_ogb_t.ogb67
         CALL cl_err('','axm-168',0)
         RETURN FALSE
      END IF
      IF g_ogb[l_ac].ogb67 = 0 THEN
         CALL cl_err('','aec-994',0)  #項次不能小於等於0
         RETURN FALSE
      END IF
      IF g_ogb[l_ac].ogb67 != g_ogb_t.ogb67 OR
         g_ogb_t.ogb67 IS NULL THEN
         IF g_argv0 = '8' THEN
            SELECT * FROM ogb_file
             WHERE ogb01 = g_ogb[l_ac].ogb66 AND ogb03 = g_ogb[l_ac].ogb67
            IF SQLCA.sqlcode = 100 THEN
               CALL cl_err3("sel","ogb_file",g_ogb[l_ac].ogb66,g_ogb[l_ac].ogb67,"axm-417","","",1)
               LET g_ogb[l_ac].ogb67 = g_ogb_t.ogb67
               RETURN FALSE
            END IF
            LET g_cnt=0
            SELECT count(*) INTO g_cnt FROM ogb_file
                WHERE ogb01 = g_oga.oga01 AND ogb67 = g_ogb[l_ac].ogb67
                  AND (ogb1005 ='1'
                  OR (ogb1005 = '2'
                  AND ogb03 <'9001'))
            IF g_cnt > 0 THEN
                LET g_ogb[l_ac].ogb67 = g_ogb_t.ogb67
                CALL cl_err('',-239,0) RETURN FALSE
            END IF
            #CHI-D90046 -- add start --
            IF NOT t600_set_ogb() THEN
               LET g_ogb[l_ac].* = g_ogb_t.*
               CALL cl_err(g_msg,'axm1201',1)
               RETURN FALSE
            END IF
            #CHI-D90046 -- add end --
         END IF
         IF g_aza.aza50 = 'Y' THEN
            IF g_ogb[l_ac].ogb67 >'9001' THEN
               LET g_ogb[l_ac].ogb67 =NULL
               RETURN FALSE
            END IF
         END IF
      END IF
   END IF
   CALL t600_set_no_entry_b1('')
   CALL t600_set_no_entry_b1('s')
   CALL t600_set_entry_ogb092()
   CALL t600_set_no_entry_ogb092()
   RETURN TRUE
END FUNCTION

