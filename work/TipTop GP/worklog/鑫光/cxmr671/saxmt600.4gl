            AND omevoid<>'Y'
         #IF g_cnt != 0 THEN #MOD-C10027 mark
         IF l_cnt != 0 THEN  #MOD-C10027 add
            RETURN FALSE
         END IF
      END FOREACH
   END IF
   RETURN TRUE
END FUNCTION
