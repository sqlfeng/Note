            AND imgg03=l_ogg[i].ogg091   AND imgg04=l_ogg[i].ogg092
            AND imgg09=l_ogg[i].ogg10
      END IF
      IF l_ima906 = '3' THEN
         IF l_ogg[i].ogg20 = '2' THEN
            SELECT imgg10 INTO l_ogg[i].img10 FROM imgg_file
             WHERE imgg01=l_ogg[i].ogg17 AND imgg02=l_ogg[i].ogg09
               AND imgg03=l_ogg[i].ogg091   AND imgg04=l_ogg[i].ogg092
               AND imgg09=l_ogg[i].ogg10
         ELSE
            SELECT img10 INTO l_ogg[i].img10 FROM img_file
             WHERE img01=l_ogg[i].ogg17 AND img02=l_ogg[i].ogg09
               AND img03=l_ogg[i].ogg091   AND img04=l_ogg[i].ogg092
         END IF
      END IF
      IF cl_null(l_ogg[i].img10) THEN LET l_ogg[i].img10 = 0 END IF
      LET i = i + 1
   END FOREACH
   CALL l_ogg.deleteElement(i)
   LET l_i=i-1
   DISPLAY l_i TO cn2

   SELECT SUM(ogg12),SUM(ogg16) INTO l_ogg12_t1,l_ogg16_t1 FROM ogg_file
    WHERE ogg01 = g_oga.oga01 AND ogg03 = g_ogb[l_ac].ogb03
      AND ogg20 = '1'
   SELECT SUM(ogg12),SUM(ogg16) INTO l_ogg12_t2,l_ogg16_t2 FROM ogg_file
    WHERE ogg01 = g_oga.oga01 AND ogg03 = g_ogb[l_ac].ogb03
      AND ogg20 = '2'

   DISPLAY l_ogg12_t1 TO ogg12t_1
   DISPLAY l_ogg16_t1 TO ogg16t_1
   DISPLAY l_ogg12_t2 TO ogg12t_2
   DISPLAY l_ogg16_t2 TO ogg16t_2
   DISPLAY g_ogb[l_ac].ogb912 TO ogb12t_1
   DISPLAY g_ogb[l_ac].ogb915 TO ogb12t_2

   CALL cl_set_act_visible("cancel", FALSE)
   LET i=ARR_CURR()
   IF g_action_choice="qry_mntn_inv_detail" THEN
      DISPLAY ARRAY l_ogg TO s_ogg.* ATTRIBUTE(COUNT=g_rec_b1,UNBUFFERED)

      BEFORE ROW
        LET i=ARR_CURR()
      ON IDLE g_idle_seconds
         CALL cl_on_idle()
         CONTINUE DISPLAY

      ON ACTION about
         CALL cl_about()

      ON ACTION help
         CALL cl_show_help()

      ON ACTION controlg
         CALL cl_cmdask()

      ON ACTION qry_lot
         LET g_ima918 = ''
         LET g_ima921 = ''
         LET g_ima906 = ''   #MOD-BC0207 add
         SELECT ima918,ima921,ima906 INTO g_ima918,g_ima921,g_ima906 #MOD-BC0207 add ima906
           FROM ima_file
          WHERE ima01 = g_ogb[l_ac].ogb04
            AND imaacti = "Y"

         IF g_ima918 = "Y" OR g_ima921 = "Y" THEN
            IF g_sma.sma150 = 'N' THEN                                           #CHI-E70002 add
               IF g_argv0='2' OR g_argv0='4' OR g_argv0='6' OR g_argv0='8' OR g_argv0='9' OR g_argv0='A' OR ((g_argv0 ='1' OR g_argv0='5') AND g_oaz.oaz81 = 'Y') THEN
                  IF g_ima906 = '3' AND l_ogg[i].ogg20 = '2' THEN #MOD-BC0207 add
                  ELSE #MOD-BC0207 add
                     CALL s_mod_lot(g_prog,g_oga.oga01,g_ogb[l_ac].ogb03,l_ogg[i].ogg18,
                                   g_ogb[l_ac].ogb04,l_ogg[i].ogg09,
                                   l_ogg[i].ogg091,l_ogg[i].ogg092,
                                   l_ogg[i].ogg10,l_ogg[i].ogg15,l_ogg[i].ogg15_fac,
                                   l_ogg[i].ogg12,'','QRY','-1')
                         RETURNING l_r,g_qty
                     IF l_r = "Y" THEN
                        LET l_ogg[i].ogg12 = g_qty
                     END IF
                  END IF #MOD-BC0207 add
               END IF
            END IF       #CHI-E70002 add
         END IF
      END DISPLAY

   END IF

   CLOSE WINDOW t6006_w
   IF INT_FLAG THEN
      LET INT_FLAG = 0
      RETURN
   END IF
END FUNCTION

FUNCTION t600_b_ogg2()   # 庫存異動明細(ogg_file)輸入
  DEFINE r_ogg            RECORD LIKE ogg_file.*
  DEFINE r_ogc            RECORD LIKE ogc_file.*
  DEFINE l_ogg            DYNAMIC ARRAY OF RECORD
         ogg17            LIKE ogg_file.ogg17,
         ogg20            LIKE ogg_file.ogg20,
         ogg09            LIKE ogg_file.ogg09,
         ogg091           LIKE ogg_file.ogg091,
         ogg092           LIKE ogg_file.ogg092,
         ogg12            LIKE ogg_file.ogg12,
         ogg13            LIKE ogg_file.ogg13, #FUN-C50097
         ogg10            LIKE ogg_file.ogg10,
         #img10           LIKE type_file.num10,   #MOD-BC0116 mark
         img10            LIKE img_file.img10,    #MOD-BC0116 add
         ogg15            LIKE ogg_file.ogg15,
         ogg15_fac        LIKE ogg_file.ogg15_fac,
         ogg16            LIKE ogg_file.ogg16,
         ogg18            LIKE ogg_file.ogg18
                          END RECORD
  DEFINE l_ogg12_t1       LIKE ogg_file.ogg12
  DEFINE l_ogg16_t1       LIKE ogg_file.ogg16
  DEFINE l_ogg12_t2       LIKE ogg_file.ogg12
  DEFINE l_ogg16_t2       LIKE ogg_file.ogg16
  DEFINE l_img21          LIKE img_file.img21
  DEFINE l_ima906         LIKE ima_file.ima906
  DEFINE l_img09          LIKE img_file.img09
  DEFINE i,j,s,l_i,k      LIKE type_file.num5,
         l_allow_insert   LIKE type_file.num5,                #可新增否
         l_allow_delete   LIKE type_file.num5                 #可刪除否
   DEFINE l_ogg18      LIKE ogg_file.ogg18
  DEFINE l_pmd            LIKE type_file.chr1
  DEFINE l_msg            STRING
  DEFINE l_msg1           STRING
  DEFINE l_msg2           STRING
  DEFINE l_msg3           STRING      #TQC-C50131
  DEFINE l_cnt            LIKE type_file.num5
  DEFINE l_bno            LIKE rvbs_file.rvbs08
  DEFINE l_oea00          LIKE oea_file.oea00,
         l_oea11          LIKE oea_file.oea11,
         l_oea12          LIKE oea_file.oea12,
         l_oga16          LIKE oga_file.oga16
  DEFINE l_ocl01          LIKE ocl_file.ocl01
  DEFINE l_date           LIKE type_file.dat
  DEFINE  l_ocl02      LIKE ocl_file.ocl02
  DEFINE  l_ocl03      LIKE ocl_file.ocl03
  DEFINE  l_ocl04      LIKE ocl_file.ocl04
  DEFINE l_ocm01       LIKE ocm_file.ocm01
  DEFINE l_ogg17_t     LIKE ogg_file.ogg17
  DEFINE l_ocm06       LIKE ocm_file.ocm06
  DEFINE l_ogg13_t1       LIKE ogg_file.ogg12 #FUN-C50097
  DEFINE l_ogg13_t2       LIKE ogg_file.ogg12 #FUN-C50097
  DEFINE l_imaicd08    LIKE imaicd_file.imaicd08    #MOD-F30164 add

   LET l_oea00 = ''
   LET l_oea11 = ''
   LET l_oea12 = ''
   LET l_oga16 = ''
   IF g_ogb[l_ac].ogb17 IS NULL THEN RETURN END IF
   IF g_ogb[l_ac].ogb17 = 'N' THEN RETURN END IF

   OPEN WINDOW t6006_w AT p_row,p_col WITH FORM "axm/42f/axmt600e_1"
      ATTRIBUTE (STYLE = g_win_style CLIPPED)

    CALL cl_ui_locale("axmt600e_1")
#FUN-C50097 ADD BEGIN
   CALL cl_set_comp_visible("ogg13",FALSE)
   IF g_oga.oga09 ='8' AND g_oaz.oaz94 ='Y' THEN #TQC-C70206       #FUN-D70016 remove g_aza.aza26 ='2' AND
      CALL cl_set_comp_visible("ogg13",TRUE)
   END IF
#FUN-C50097 ADD END
   IF g_sma.sma122 = '1' THEN
      CALL cl_getmsg('asm-303',g_lang) RETURNING l_msg1
      CALL cl_getmsg('asm-302',g_lang) RETURNING l_msg2
      LET l_msg = '1:',l_msg1 CLIPPED,',2:',l_msg2 CLIPPED
      CALL cl_set_combo_items('ogg20','1,2',l_msg)
   END IF

   IF g_sma.sma122 ='2' THEN
      CALL cl_getmsg('asm-326',g_lang) RETURNING l_msg1
      CALL cl_getmsg('asm-304',g_lang) RETURNING l_msg2
      LET l_msg = '1:',l_msg1 CLIPPED,',2:',l_msg2 CLIPPED
      CALL cl_set_combo_items('ogg20','1,2',l_msg)
   END IF

   CALL t600b_set_no_entry_ogg()

   IF g_oaz.oaz42 = '2' THEN
      CALL cl_getmsg('axm-041',g_lang) RETURNING g_msg
      CALL cl_set_comp_att_text("a",g_msg CLIPPED)
   END IF

   #抓取ogg_file中的資料
   DECLARE t6006_ogg2_c CURSOR FOR
    SELECT ogg17,ogg20,ogg09,ogg091,ogg092,ogg12,ogg13,ogg10,0,ogg15,ogg15_fac,ogg16,ogg18
      FROM ogg_file #FUN-C50097 add ogg13 ogg13,
     WHERE ogg01 = g_oga.oga01 AND ogg03 = g_ogb[l_ac].ogb03
     ORDER BY ogg20

   CALL l_ogg.clear()

   SELECT ima906 INTO l_ima906 FROM ima_file WHERE ima01=g_ogb[l_ac].ogb04

   LET i = 1
      LET l_i = 1
   FOREACH t6006_ogg2_c INTO l_ogg[i].*
      IF STATUS THEN
         CALL cl_err('foreach ogg',STATUS,0)
         EXIT FOREACH
      END IF
      IF l_ima906 = '1' THEN
         SELECT img10 INTO l_ogg[i].img10 FROM img_file
          WHERE img01=l_ogg[i].ogg17 AND img02=l_ogg[i].ogg09
            AND img03=l_ogg[i].ogg091   AND img04=l_ogg[i].ogg092
      END IF
      IF l_ima906 = '2' THEN
         SELECT imgg10 INTO l_ogg[i].img10 FROM imgg_file
          WHERE imgg01=l_ogg[i].ogg17 AND imgg02=l_ogg[i].ogg09
            AND imgg03=l_ogg[i].ogg091   AND imgg04=l_ogg[i].ogg092
            AND imgg09=l_ogg[i].ogg10
      END IF
      IF l_ima906 = '3' THEN
         IF l_ogg[i].ogg20 = '2' THEN
            SELECT imgg10 INTO l_ogg[i].img10 FROM imgg_file
             WHERE imgg01=l_ogg[i].ogg17 AND imgg02=l_ogg[i].ogg09
               AND imgg03=l_ogg[i].ogg091   AND imgg04=l_ogg[i].ogg092
               AND imgg09=l_ogg[i].ogg10
         ELSE
            SELECT img10 INTO l_ogg[i].img10 FROM img_file
             WHERE img01=l_ogg[i].ogg17 AND img02=l_ogg[i].ogg09
               AND img03=l_ogg[i].ogg091   AND img04=l_ogg[i].ogg092
         END IF
      END IF
      IF cl_null(l_ogg[i].img10) THEN LET l_ogg[i].img10 = 0 END IF
      LET i = i + 1
   END FOREACH
   CALL l_ogg.deleteElement(i)
      LET i = i - 1

   #判斷是否用替代原則
   IF g_oaz.oaz42 = '1' THEN
      DECLARE t600b_c7_1 CURSOR FOR
         SELECT ocl01,ocl06,ocl04,ocl02,ocl03  FROM ocl_file
         #WHERE (ocl06<=g_oga.oga02 OR ocl07>=g_oga.oga02) #MOD-D50072 mark
	 #MOD-D50072 add start -----
          WHERE (ocl06 <= g_oga.oga02)
            AND (ocl07 > g_oga.oga02 OR ocl07 IS NULL)
         #MOD-D50072 add end   -----
          ORDER BY ocl01

      FOREACH t600b_c7_1 INTO l_ocl01,l_date,l_ocl04,l_ocl02,l_ocl03
         IF l_ocl04 = g_ogb[l_ac].ogb04[l_ocl02,l_ocl03] THEN
            IF NOT cl_null(l_ocl01) THEN
               EXIT FOREACH
            END IF
         END IF
      END FOREACH

      DISPLAY l_ocl01,l_date TO FORMONLY.a,FORMONLY.b

      IF i = 0 THEN

         INPUT l_ocl01,l_date WITHOUT DEFAULTS FROM a,b

            ON ACTION controlp
               CASE
                  WHEN INFIELD(a)
                     CALL cl_init_qry_var()
                     LET g_qryparam.form ="q_ocl"
                     LET g_qryparam.default1 = l_ocl01
                     CALL cl_create_qry() RETURNING l_ocl01
                     DISPLAY l_ocl01 TO FORMONLY.a
                     NEXT FIELD a
               END CASE

            ON IDLE g_idle_seconds
               CALL cl_on_idle()
               CONTINUE INPUT

            ON ACTION about
               CALL cl_about()

            ON ACTION help
               CALL cl_show_help()

            ON ACTION controlg
               CALL cl_cmdask()

         END INPUT

      END IF
   END IF

   #判斷是否用替代群組
   IF g_oaz.oaz42 = '2' THEN
      DECLARE t600b_c6_1 CURSOR FOR
         SELECT ocm01,ocm03 FROM ocm_file
          WHERE ocm02=g_ogb[l_ac].ogb04
           #AND (ocm03<=g_oga.oga02 OR ocm04>=g_oga.oga02) #MOD-D50072 mark
	   #MOD-D50072 add start -----
            AND (ocm03 <= g_oga.oga02)
            AND (ocm04 > g_oga.oga02 OR ocm04 IS NULL)
           #MOD-D50072 add end   -----
            AND ocm05 = g_oga.oga03
          ORDER BY ocm01

      FOREACH t600b_c6_1 INTO l_ocm01,l_date
         IF NOT cl_null(l_ocm01) THEN EXIT FOREACH END IF
      END FOREACH
      #當參數設定為'替代群組'時, 先check 該客戶,替代群組是否有資料,
      #若無,則再check 客戶是'*',且滿足該替代群組的資料
      IF cl_null(l_ocm01) THEN    #表示沒有符合該客戶的替代群組,抓客戶編號是'*'的
         DECLARE t600b_c61_1 CURSOR FOR
            SELECT ocm01,ocm03 FROM ocm_file
             WHERE ocm02=g_ogb[l_ac].ogb04
              #AND (ocm03<=g_oga.oga02 OR ocm04>=g_oga.oga02) #MOD-D50072 mark
	      #MOD-D50072 add start -----
               AND (ocm03 <= g_oga.oga02)
               AND (ocm04 > g_oga.oga02 OR ocm04 IS NULL)
              #MOD-D50072 add end   -----
               AND ocm05 = '*'
             ORDER BY ocm01
         FOREACH t600b_c61_1 INTO l_ocm01,l_date
            IF NOT cl_null(l_ocm01) THEN EXIT FOREACH END IF
         END FOREACH
      END IF
      DISPLAY l_ocm01,l_date TO FORMONLY.a,FORMONLY.b

      IF i = 0 THEN

      INPUT l_ocm01,l_date WITHOUT DEFAULTS FROM a,b
        ON ACTION controlp
        CASE
            WHEN INFIELD(a)
                 CALL cl_init_qry_var()
                 LET g_qryparam.form ="q_ocm"
                 LET g_qryparam.default1 = l_ocm01
                 CALL cl_create_qry() RETURNING l_ocm01
                 DISPLAY l_ocm01 TO FORMONLY.a
                 NEXT FIELD a
            END CASE

        AFTER FIELD a
           IF cl_null(l_ocm01) THEN
              CALL cl_err('','axm-923',1)   #僅做多倉儲料出貨,不做產品替代!
           END IF

        ON IDLE g_idle_seconds
           CALL cl_on_idle()
           CONTINUE INPUT

         ON ACTION about
            CALL cl_about()

         ON ACTION help
            CALL cl_show_help()

         ON ACTION controlg
            CALL cl_cmdask()

      END INPUT
      END IF
   END IF

   #空白的話表示僅做多倉儲料出貨
   IF (g_oaz.oaz42 = '1' AND cl_null(l_ocl01)) OR
      (g_oaz.oaz42 = '2' AND cl_null(l_ocm01)) THEN   #空白的話表示僅做多倉儲料出貨
      CALL t600_ogg2_ogg()
   ELSE                       #產品替代
      IF i = 0 THEN
         IF g_oaz.oaz42 = '1' THEN
            CALL t600_ogg2_ocl(l_ocl01,l_date,'1')
         ELSE
            CALL t600_ogg2_ocm(l_ocm01,l_date,'1')
         END IF
      END IF
   END IF

   CREATE TEMP TABLE ogg_temp(
       ogg01     LIKE ogg_file.ogg01,
       ogg03     LIKE ogg_file.ogg03,
       ogg09     LIKE ogg_file.ogg09,
       ogg091    LIKE ogg_file.ogg091,
       ogg092    LIKE ogg_file.ogg092,
       ogg10     LIKE ogg_file.ogg10,
       ogg12     LIKE ogg_file.ogg12,
       ogg15     LIKE ogg_file.ogg15,
       ogg15_fac LIKE ogg_file.ogg15,
       ogg16     LIKE ogg_file.ogg16,
       ogg20     LIKE ogg_file.ogg20,
       ogg17     LIKE ogg_file.ogg17,
       ogg18     LIKE ogg_file.ogg18,
       oggplant  LIKE ogg_file.oggplant,
       ogglegal  LIKE ogg_file.ogglegal,
       ogg13     LIKE ogg_file.ogg13 ) #FUN-C50097

   DECLARE t6006_ogg2_c2_1 CURSOR FOR
    SELECT ogg17,ogg20,ogg09,ogg091,ogg092,ogg12,ogg13,ogg10,0,ogg15,ogg15_fac,ogg16,ogg18
      FROM ogg_file #FUN-C50097 OGG13 ogg13,
     WHERE ogg01 = g_oga.oga01 AND ogg03 = g_ogb[l_ac].ogb03
     ORDER BY ogg20

   CALL l_ogg.clear()

   LET i = 1
   FOREACH t6006_ogg2_c2_1 INTO l_ogg[i].*
      IF STATUS THEN
         CALL cl_err('foreach ogg',STATUS,0)
         EXIT FOREACH
      END IF
      IF l_ima906 = '1' THEN
         SELECT img10 INTO l_ogg[i].img10 FROM img_file
          WHERE img01=l_ogg[i].ogg17 AND img02=l_ogg[i].ogg09
            AND img03=l_ogg[i].ogg091   AND img04=l_ogg[i].ogg092
      END IF
      IF l_ima906 = '2' THEN
         SELECT imgg10 INTO l_ogg[i].img10 FROM imgg_file
          WHERE imgg01=l_ogg[i].ogg17 AND imgg02=l_ogg[i].ogg09
            AND imgg03=l_ogg[i].ogg091   AND imgg04=l_ogg[i].ogg092
            AND imgg09=l_ogg[i].ogg10
      END IF
      IF l_ima906 = '3' THEN
         IF l_ogg[i].ogg20 = '2' THEN
            SELECT imgg10 INTO l_ogg[i].img10 FROM imgg_file
             WHERE imgg01=l_ogg[i].ogg17 AND imgg02=l_ogg[i].ogg09
               AND imgg03=l_ogg[i].ogg091   AND imgg04=l_ogg[i].ogg092
               AND imgg09=l_ogg[i].ogg10
         ELSE
            SELECT img10 INTO l_ogg[i].img10 FROM img_file
             WHERE img01=l_ogg[i].ogg17 AND img02=l_ogg[i].ogg09
               AND img03=l_ogg[i].ogg091   AND img04=l_ogg[i].ogg092
         END IF
      END IF
      IF cl_null(l_ogg[i].img10) THEN LET l_ogg[i].img10 = 0 END IF
      LET i = i + 1
   END FOREACH
   CALL l_ogg.deleteElement(i)

   LET l_i=i-1
   DISPLAY l_i TO cn2

   SELECT SUM(ogg12),SUM(ogg16) INTO l_ogg12_t1,l_ogg16_t1 FROM ogg_file
    WHERE ogg01 = g_oga.oga01 AND ogg03 = g_ogb[l_ac].ogb03
      AND ogg20 = '1'
   SELECT SUM(ogg12),SUM(ogg16) INTO l_ogg12_t2,l_ogg16_t2 FROM ogg_file
    WHERE ogg01 = g_oga.oga01 AND ogg03 = g_ogb[l_ac].ogb03
      AND ogg20 = '2'

   DISPLAY l_ogg12_t1 TO ogg12t_1
   DISPLAY l_ogg16_t1 TO ogg16t_1
   DISPLAY l_ogg12_t2 TO ogg12t_2
   DISPLAY l_ogg16_t2 TO ogg16t_2
   DISPLAY g_ogb[l_ac].ogb912 TO ogb12t_1
   DISPLAY g_ogb[l_ac].ogb915 TO ogb12t_2

   LET l_allow_insert = cl_detail_input_auth("insert")
   LET l_allow_delete = cl_detail_input_auth("delete")

   INPUT ARRAY l_ogg WITHOUT DEFAULTS FROM s_ogg.*
         ATTRIBUTE(COUNT=l_i,MAXCOUNT=g_max_rec,UNBUFFERED,
                   INSERT ROW=l_allow_insert,DELETE ROW=l_allow_delete,APPEND ROW=l_allow_insert)

     BEFORE INPUT
         CALL t600_set_entry_ogg(l_ocm01)
         CALL t600_set_no_entry_ogg(l_ocm01)

     BEFORE ROW
        LET l_ogg17_t = l_ogg[i].ogg17
        LET i=ARR_CURR()
        LET l_pmd = ''
        IF l_i < i THEN
           LET l_ogg[i].ogg20 = '1'
           LET l_ogg[i].ogg10 = g_ogb[l_ac].ogb910
           LET l_pmd = 'a'
           DISPLAY l_ogg[i].ogg10 TO ogg10
           DISPLAY l_ogg[i].ogg20 TO ogg20
        ELSE
           IF cl_null(l_ogg[i].ogg18) OR l_ogg[i].ogg18 = 0 THEN
              SELECT MAX(ogg18)+1 INTO l_ogg[i].ogg18
                FROM ogg_file
               WHERE ogg01 = g_oga.oga01
                 AND ogg03 = g_ogb[l_ac].ogb03
           END IF
           IF cl_null(l_ogg[i].ogg09) THEN
              LET l_ogg[i].ogg09 = g_ogb[l_ac].ogb09
           END IF
           IF l_ogg[i].ogg091 IS NULL THEN
              LET l_ogg[i].ogg091= g_ogb[l_ac].ogb091
           END IF
           IF l_ogg[i].ogg092 IS NULL THEN
              LET l_ogg[i].ogg092= g_ogb[l_ac].ogb092
           END IF
           NEXT FIELD ogg12
        END IF
     BEFORE DELETE

     AFTER FIELD ogg09
           IF NOT s_chk_ware(l_ogg[i].ogg09) THEN
              NEXT FIELD ogg09
           END IF
           #MOD-D40026 add begin
           IF l_ogg[i].ogg09 = g_oaz.oaz74 OR l_ogg[i].ogg09 = g_oaz.oaz75
              OR l_ogg[i].ogg09 = g_oaz.oaz95 THEN
              CALL cl_err(l_ogg[i].ogg09,'axm-815',1)
              NEXT FIELD ogg09
           END IF
           #MOD-D40026 add end
     AFTER FIELD ogg17
        IF NOT cl_null(l_ogg[i].ogg17) THEN
            IF NOT s_chk_item_no(l_ogg[i].ogg17,"") THEN
               CALL cl_err('',g_errno,1)
               LET l_ogg[i].ogg17= l_ogg17_t
               NEXT FIELD ogg17
            END IF
           IF cl_null(l_ogg17_t) OR l_ogg[i].ogg17 != l_ogg17_t THEN
              DELETE FROM ogg_temp
              IF g_oaz.oaz42 = '1' THEN
                 CALL t600_ogg2_ocl(l_ocl01,l_date,'2')
              ELSE
                 CALL t600_ogg2_ocm(l_ocm01,l_date,'2')
              END IF
              IF g_oaz.oaz42 = '1' THEN
                 SELECT * FROM yyy_tmp,ima_file
                  WHERE ima01=item AND ima01=l_ogg[i].ogg17
              ELSE
                 SELECT COUNT(*) INTO g_cnt FROM ogg_temp
                 DISPLAY g_cnt
                 SELECT ogg17 INTO l_ogg[i].ogg17 FROM ogg_temp
                  WHERE ogg17=l_ogg[i].ogg17
              END IF
              IF STATUS = 100 THEN
                 IF g_oaz.oaz42 = '1' THEN
                    CALL cl_err(l_ogg[i].ogg17,'axm-527',0)
                 ELSE
                    LET l_cnt=0
                    SELECT COUNT(*) INTO l_cnt
                      FROM ocm_file
                     WHERE ocm01=l_ocm01
                       AND ocm02=l_ogg[i].ogg17
                       AND ocm03<=l_date
                    IF l_cnt>0 THEN
                       CALL cl_err('','axm-529',0)
                    ELSE
                       CALL cl_err(l_ogg[i].ogg17,'axm-528',0)
                    END IF
                 END IF
                 NEXT FIELD ogg17
              END IF
           END IF
        END IF

      ON ACTION controlp
         CASE
            WHEN INFIELD(ogg09)
               IF g_azw.azw04='2' THEN
                  CALL q_img42(FALSE,FALSE,l_ogg[i].ogg17,'','','','A','1',g_oga.ogaplant)
                     RETURNING l_ogg[i].ogg09,l_ogg[i].ogg091,l_ogg[i].ogg092
               ELSE
                  #FUN-C30300---begin
                  LET g_ima906 = NULL
                  SELECT ima906 INTO g_ima906 FROM ima_file
                   WHERE ima01 = l_ogg[i].ogg17
                 #MOD-F30164 ---start add---
