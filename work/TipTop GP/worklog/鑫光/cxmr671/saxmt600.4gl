      LET l_gfeacti = NULL
      SELECT gfeacti
        INTO l_gfeacti
        FROM gfe_file
       WHERE gfe01 = p_ogb916
      CASE
         WHEN SQLCA.sqlcode = 100
            LET g_errno = 'art1068'  #無此單位，請重新輸入！
