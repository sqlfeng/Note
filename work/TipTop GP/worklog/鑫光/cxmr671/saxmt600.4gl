# M008 151111 By TSD.Ruby------(s)
# ogb31欄位檢核
FUNCTION t600_chk_ogb31_all(p_cmd)
   DEFINE l_flag   LIKE type_file.chr1
   DEFINE p_cmd    LIKE type_file.chr1
   DEFINE l_oga72  LIKE oga_file.oga72
   DEFINE l_oat05  LIKE oat_file.oat05
   DEFINE l_occ20  LIKE occ_file.occ20
   DEFINE l_occ21  LIKE occ_file.occ21
   DEFINE l_occ22  LIKE occ_file.occ22
   DEFINE l_oea31  LIKE oea_file.oea31

   WHILE TRUE
      LET l_flag = 'Y'
      IF NOT cl_null(g_oga.oga011) OR NOT cl_null(g_oga.oga16) THEN
         IF cl_null(g_ogb[l_ac].ogb31) THEN
            CALL cl_err('','abx-067',0)
            LET l_flag = 'N'
            EXIT WHILE
         END IF
      END IF
      IF cl_null(g_oga.oga16) THEN
         IF NOT t600_chk_ogb31_last() THEN
            LET l_flag = 'N'
            EXIT WHILE
         END IF
         IF g_azw.azw04 = '2' THEN
            IF g_oga.oga85 = '1' AND NOT cl_null(g_ogb[l_ac].ogb31) THEN
               CALL cl_err('','axm1221',0)
               LET l_flag = 'N'
               EXIT WHILE
            END IF
         END IF
      END IF

      IF (g_ogb[l_ac].ogb31!=g_ogb_t.ogb31 OR g_ogb_t.ogb31 IS NULL) THEN
          IF NOT t600_chk_ogb31(p_cmd) THEN
             LET l_flag = 'N'
             EXIT WHILE
          END IF
          IF NOT t600_chk_oea() THEN
             LET l_flag = 'N'
             EXIT WHILE
          END IF
          IF NOT cl_null(g_ogb[l_ac].ogb32) THEN
             CASE t600_chk_ogb32(p_cmd)
                WHEN "ogb31"
                   LET l_flag = 'N'
                   EXIT WHILE
                WHEN "ogb32"
                   LET l_flag = 'N'
                   EXIT WHILE
             END CASE
          END IF
          IF cl_null(g_oga.oga31) THEN
             SELECT oea31 INTO g_oga.oga31
               FROM oea_file WHERE oea01 = g_ogb[l_ac].ogb31
          END IF
      ELSE
         IF (NOT cl_null(g_ogb[l_ac].ogb31) AND NOT cl_null(g_ogb[l_ac].ogb32)) AND (g_ogb[l_ac].ogb32!=g_ogb_o.ogb32) THEN
             CASE t600_chk_ogb32(p_cmd)
                WHEN "ogb31"
                   LET l_flag = 'N'
                   EXIT WHILE
                WHEN "ogb32"
                   LET l_flag = 'N'
                   EXIT WHILE
             END CASE
         END IF
      END IF

      IF (l_ac = 1 AND cl_null(g_oga.oga16) AND g_oga.oga65 = 'Y'
          AND g_argv0 MATCHES '[12]') THEN
          SELECT occ20,occ21,occ22
            INTO l_occ20,l_occ21,l_occ22
            FROM occ_file WHERE occ01 = g_oga.oga04
          SELECT oea31
            INTO l_oea31
            FROM oea_file WHERE oea01 = g_ogb[l_ac].ogb31
          IF cl_null(l_occ20) THEN  LET l_occ20 =' ' END IF
          IF cl_null(l_occ21) THEN  LET l_occ21 =' ' END IF
          IF cl_null(l_occ22) THEN  LET l_occ22 =' ' END IF
          SELECT oat05
             INTO l_oat05
             FROM oat_file WHERE oat01 = l_occ20 AND oat02 = l_occ21 AND oat03 = l_occ22 AND oat04 = l_oea31
          IF l_oat05 = 0 THEN
             SELECT oat05
                INTO l_oat05
                FROM oat_file WHERE oat01 = l_occ20 AND oat02 = l_occ21 AND oat03 = ' ' AND oat04 = l_oea31
             IF l_oat05 = 0 THEN
                SELECT oat05
                  INTO l_oat05
                  FROM oat_file WHERE oat01 = l_occ20 AND oat02 = ' ' AND oat03 = ' ' AND oat04 = l_oea31
             END IF
          END IF
          LET l_oga72 = g_oga.oga02 + l_oat05
          IF(g_oga.oga72 <> l_oga72) THEN
             CALL cl_confirm('axm1032') RETURNING g_chr
             IF g_chr='1' THEN
                LET g_oga.oga72 = l_oga72  ##預計簽收日
                DISPLAY g_oga.oga72 TO oga72
                UPDATE oga_file SET oga72 = g_oga.oga72
                WHERE oga01 = g_oga.oga01
                COMMIT WORK
             END IF
          END IF
       END IF
      LET g_ogb_o.ogb31 = g_ogb[l_ac].ogb31 #MOD-B90214 add #CHI-EA0043 mark
      IF g_azw.azw04 = '2' THEN
         CALL t600_ogb48_ogb49_chk()
         IF NOT cl_null(g_errno) THEN
            CALL cl_err('',g_errno,0)
            LET g_ogb[l_ac].ogb31 = g_ogb_t.ogb31
            LET l_flag = 'N'
            EXIT WHILE
         END IF
      END IF
      IF g_aza.aza115='Y' AND g_argv0 MATCHES '[2456A]' AND NOT t600_chk_ogb1001(p_cmd) THEN
         LET l_flag = 'N'
         EXIT WHILE
      END IF
      IF g_aza.aza115='Y' AND g_argv0 MATCHES '[89]' AND NOT t600_chk_ogb65(p_cmd) THEN
         LET l_flag = 'N'
         EXIT WHILE
      END IF
      IF NOT cl_null(g_ogb[l_ac].ogb32) AND NOT cl_null(g_ogb[l_ac].ogb31) THEN
         IF p_cmd = 'a' OR (p_cmd = 'u' AND g_ogb[l_ac].ogb31 <> g_ogb_t.ogb31) THEN
            IF g_azw.azw04 = '2' AND NOT s_industry("slk") THEN
               CALL t600_ins_rxh(g_oga.oga01,g_ogb[l_ac].ogb03,g_ogb[l_ac].ogb31,g_ogb[l_ac].ogb32)
            END IF
         END IF
         LET g_ogb_o.ogb31 = g_ogb[l_ac].ogb31 #CHI-EA0043 add
      END IF
      EXIT WHILE
   END WHILE
   RETURN l_flag
END FUNCTION

# ogb32欄位檢核
FUNCTION t600_chk_ogb32_all(p_cmd)
   DEFINE l_flag   LIKE type_file.chr1
   DEFINE p_cmd    LIKE type_file.chr1
   DEFINE l_ima918 LIKE ima_file.ima918
   DEFINE l_ima921 LIKE ima_file.ima921
   DEFINE l_ima928 LIKE ima_file.ima928
   DEFINE g_aza50  LIKE aza_file.aza50

   WHILE TRUE
      LET l_flag = 'Y'
      IF cl_null(g_ogb[l_ac].ogb32) THEN
         IF NOT cl_null(g_ogb[l_ac].ogb31) THEN
            CALL cl_err('','atm-243',0)
            LET l_flag = 'N'
            EXIT WHILE
         END IF
      END IF

      IF NOT t600_chk_oea() THEN
         LET l_flag = 'N'
         EXIT WHILE
      END IF
      IF g_azw.azw04 = '2' THEN
        CALL t600_ogb48_ogb49_chk()
        IF NOT cl_null(g_errno) THEN
            CALL cl_err('',g_errno,0)
            IF NOT cl_null(g_ogb[l_ac].ogb32) THEN
               LET l_flag = 'N'
               EXIT WHILE
            ELSE
               LET g_ogb_o.ogb32 = g_ogb[l_ac].ogb32
               LET l_flag = 'N'
               EXIT WHILE
            END IF
        END IF
     END IF
     IF NOT cl_null(g_ogb[l_ac].ogb31) AND NOT cl_null(g_ogb[l_ac].ogb32) THEN
        CALL cl_set_comp_entry("ogb04,ogb05,ogb11,ogb1004,ogb13,ogb14,ogb14t,ogb916,ogb1003,ogb1004,ogb1006,ogb913,ogb910,ogb41,ogb42,ogb43" ,FALSE)
     ELSE
        CALL cl_set_comp_entry("ogb1001,ogb04,ogb05,ogb11,ogb13,ogb14,ogb14t,ogb1001,ogb1003,ogb1004,ogb1006,ogb913,ogb910,ogb41,ogb42,ogb43" ,TRUE)
     END IF
     IF (g_ogb[l_ac].ogb32!=g_ogb_o.ogb32 OR g_ogb_o.ogb32 IS NULL) THEN #MOD-B90214
        CASE t600_chk_ogb32(p_cmd)
           WHEN "ogb31"
              LET l_flag = 'N'
              EXIT WHILE
           WHEN "ogb32"
              LET l_flag = 'N'
              EXIT WHILE
        END CASE
     END IF
     SELECT aza50 INTO g_aza50 FROM aza_file
     IF g_aza50 = 'Y' THEN
        LET g_cnt = 0
        SELECT COUNT(*) INTO g_cnt FROM oeb_file
         WHERE oeb01 = g_ogb[l_ac].ogb31 and oeb03 = g_ogb[l_ac].ogb32
           AND (oeb935 IS NOT NULL) AND (oeb936 IS NOT NULL) AND g_ogb[l_ac].ogb1012 = 'Y'
           IF g_cnt > 0 THEN
              CALL cl_err('','axm-083',0)
              LET l_flag = 'N'
              EXIT WHILE
           END IF
     END IF

     CALL t600_set_no_entry_b1(p_cmd)
     CALL t600_set_entry_ogb092()
     CALL t600_set_no_entry_ogb092()
     IF g_argv0 = '1' OR g_argv0 = '5' THEN
        IF NOT cl_null(g_ogb[l_ac].ogb04) THEN
           SELECT ima928,ima918,ima921 INTO l_ima928,l_ima918,l_ima921 FROM ima_file
            WHERE ima01 = g_ogb[l_ac].ogb04
           IF l_ima928 = 'Y' AND ( l_ima918 = 'Y' OR l_ima921 = 'Y' ) THEN
              CALL cl_err(g_ogb[l_ac].ogb04,'axm1189',1)
              LET l_flag = 'N'
              EXIT WHILE
           END IF
        END IF
     ELSE
         IF NOT cl_null(g_ogb[l_ac].ogb31) AND NOT cl_null(g_ogb[l_ac].ogb32) THEN
           CALL t600_qry_ima929(p_cmd)
         END IF
      END IF
      IF NOT cl_null(g_ogb[l_ac].ogb32) AND NOT cl_null(g_ogb[l_ac].ogb31) THEN
         IF p_cmd = 'a' OR (p_cmd = 'u' AND g_ogb[l_ac].ogb32 <> g_ogb_t.ogb32) THEN
            IF g_azw.azw04 = '2' AND NOT s_industry("slk") THEN
               CALL t600_ins_rxh(g_oga.oga01,g_ogb[l_ac].ogb03,g_ogb[l_ac].ogb31,g_ogb[l_ac].ogb32)
            END IF
         END IF
      END IF
      EXIT WHILE
   END WHILE
   RETURN l_flag
END FUNCTION
