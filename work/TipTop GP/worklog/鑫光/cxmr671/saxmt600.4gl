#重新計算計價數量
FUNCTION t600_set_d_ogb917(p_ogb04,p_ogb914,p_ogb915,p_ogb911,p_ogb912,
                           p_ogb12,p_ogb05,p_ogb916)
   DEFINE p_ogb04  LIKE ogb_file.ogb04
   DEFINE p_ogb914 LIKE ogb_file.ogb914
   DEFINE p_ogb915 LIKE ogb_file.ogb915
   DEFINE p_ogb911 LIKE ogb_file.ogb911
   DEFINE p_ogb912 LIKE ogb_file.ogb912
   DEFINE p_ogb12  LIKE ogb_file.ogb12
   DEFINE p_ogb05  LIKE ogb_file.ogb05
   DEFINE p_ogb916 LIKE ogb_file.ogb916
   DEFINE l_ogb917 LIKE ogb_file.ogb917
   DEFINE l_ima25  LIKE ima_file.ima25,     #ima單位
          l_ima31  LIKE ima_file.ima31,     #ima單位
          l_ima906 LIKE ima_file.ima906,
          l_fac2   LIKE img_file.img21,     #第二轉換率
          l_qty2   LIKE img_file.img10,     #第二數量
          l_fac1   LIKE img_file.img21,     #第一轉換率
          l_qty1   LIKE img_file.img10,     #第一數量
          l_tot    LIKE img_file.img10,     #計價數量
          l_factor LIKE ima_file.ima31_fac

   LET l_ima25 = NULL
   LET l_ima31 = NULL
   LET l_ima906 = NULL
   SELECT ima25,ima31,ima906
     INTO l_ima25,l_ima31,l_ima906
     FROM ima_file
    WHERE ima01 = p_ogb04
   IF SQLCA.sqlcode = 100 THEN
      IF p_ogb04 MATCHES 'MISC*' THEN
         SELECT ima25,ima31,ima906
           INTO l_ima25,l_ima31,l_ima906
           FROM ima_file
          WHERE ima01 = 'MISC'
      END IF
   END IF
   IF cl_null(l_ima31) THEN LET l_ima31 = l_ima25 END IF

   LET l_fac2 = p_ogb914
   LET l_qty2 = p_ogb915
   IF g_sma.sma115 = 'Y' THEN
      LET l_fac1 = p_ogb911
      LET l_qty1 = p_ogb912
   ELSE
      LET l_fac1 = 1
      LET l_qty1 = p_ogb12
      CALL s_umfchk(p_ogb04,p_ogb05,l_ima31)
         RETURNING g_cnt,l_fac1
      IF g_cnt = 1 THEN
         LET l_fac1 = 1
      END IF
   END IF
   IF cl_null(l_fac1) THEN LET l_fac1 = 1 END IF
   IF cl_null(l_qty1) THEN LET l_qty1 = 0 END IF
   IF cl_null(l_fac2) THEN LET l_fac2 = 1 END IF
   IF cl_null(l_qty2) THEN LET l_qty2 = 0 END IF

   IF g_sma.sma115 = 'Y' THEN
      CASE l_ima906
         WHEN '1' LET l_tot = l_qty1 * l_fac1
         WHEN '2' LET l_tot = l_qty1 * l_fac1 + l_qty2 * l_fac2
         WHEN '3' LET l_tot = l_qty1 * l_fac1
         OTHERWISE
            EXIT CASE
      END CASE
   ELSE  #不使用雙單位
      LET l_tot = l_qty1 * l_fac1
   END IF
   IF cl_null(l_tot) THEN LET l_tot = 0 END IF

   IF g_sma.sma115 = 'Y' THEN
      CALL s_umfchk(p_ogb04,p_ogb05,p_ogb916)
         RETURNING g_cnt,l_factor
   ELSE
      CALL s_umfchk(p_ogb04,l_ima31,p_ogb916)
         RETURNING g_cnt,l_factor
   END IF
   IF g_cnt = 1 THEN
      LET l_factor = 1
   END IF
   LET l_tot = l_tot * l_factor

   LET l_ogb917 = l_tot
   LET l_ogb917 = s_digqty(l_ogb917,p_ogb916)

   IF p_ogb05 = p_ogb916 AND g_sma.sma115 = 'N' THEN
      LET l_ogb917 = p_ogb12
   END IF

   RETURN l_ogb917
END FUNCTION

#重新計算金額
FUNCTION t600_set_d_ogb14(p_ogb03,p_ogb04,
                          p_ogb916,p_ogb917,p_ogb12,
                          p_ogb13,p_ogb37)
   DEFINE p_ogb03   LIKE ogb_file.ogb03
   DEFINE p_ogb04   LIKE ogb_file.ogb04
   DEFINE p_ogb916  LIKE ogb_file.ogb916
   DEFINE p_ogb917  LIKE ogb_file.ogb917
   DEFINE p_ogb12   LIKE ogb_file.ogb12
   DEFINE p_ogb13   LIKE ogb_file.ogb13
   DEFINE p_ogb37   LIKE ogb_file.ogb37
   DEFINE l_qty     LIKE ogb_file.ogb917
   DEFINE l_rtz04   LIKE rtz_file.rtz04
   DEFINE l_rtz06   LIKE rtz_file.rtz06
   DEFINE l_ogb1005 LIKE ogb_file.ogb1005
   DEFINE l_ogb1012 LIKE ogb_file.ogb1012
   DEFINE l_ogb1006 LIKE ogb_file.ogb1006
   DEFINE l_ogb14   LIKE ogb_file.ogb14
   DEFINE l_ogb14t  LIKE ogb_file.ogb14t

   LET l_ogb1005 = NULL
   LET l_ogb1012 = NULL
   LET l_ogb1006 = NULL
   SELECT ogb1005,ogb1012,ogb1006
     INTO l_ogb1005,l_ogb1012,l_ogb1006
     FROM ogb_file
    WHERE ogb01 = g_oga.oga01
      AND ogb03 = p_ogb03

   LET l_rtz04 = NULL
   LET l_rtz06 = NULL
   SELECT rtz04, rtz06
     INTO l_rtz04,l_rtz06
     FROM rtz_file
    WHERE rtz01 = g_oga.ogaplant

   IF l_ogb1005 = '1' THEN
      IF g_sma.sma116 MATCHES '[23]' THEN
         LET l_qty = p_ogb917
      ELSE
         LET l_qty = p_ogb12
      END IF

      IF l_qty IS NULL THEN
         LET l_qty = 0
      END IF

      LET t_azi03 = 0
      LET t_azi04 = 0
      SELECT azi03,azi04
        INTO t_azi03,t_azi04
        FROM azi_file
       WHERE azi01 = g_oga.oga23

      CALL cl_digcut(p_ogb13,t_azi03) RETURNING p_ogb13
      CALL cl_digcut(p_ogb37,t_azi03) RETURNING p_ogb37

      IF cl_null(l_ogb1012) THEN LET l_ogb1012 = 'N' END IF

      IF l_ogb1012 = 'N' THEN
         IF g_azw.azw04 = '2' AND NOT s_industry("slk") THEN
            CALL t620_sub(p_ogb04,l_rtz04,g_oga.oga213,l_qty,p_ogb13,t_azi03)
               RETURNING l_ogb14,l_ogb14t
         ELSE
            IF g_oga.oga213 = 'N'  THEN
               LET l_ogb14 = t600_amount(l_qty,p_ogb13,l_ogb1006,t_azi03)
               CALL cl_digcut(l_ogb14 ,t_azi04) RETURNING l_ogb14
               LET l_ogb14t = l_ogb14 * (1 + g_oga.oga211 / 100)
               CALL cl_digcut(l_ogb14t,t_azi04) RETURNING l_ogb14t
            ELSE
               LET l_ogb14t = t600_amount(l_qty,p_ogb13,l_ogb1006,t_azi03)
               CALL cl_digcut(l_ogb14t,t_azi04) RETURNING l_ogb14t
               LET l_ogb14 = l_ogb14t / (1 + g_oga.oga211 / 100)
               CALL cl_digcut(l_ogb14, t_azi04) RETURNING l_ogb14
            END IF
         END IF
      ELSE
         LET l_ogb14  = 0
         LET l_ogb14t = 0
      END IF
   END IF

   RETURN l_ogb14,l_ogb14t,p_ogb13,p_ogb37
END FUNCTION

