FUNCTION t600_list_fill()
  DEFINE l_oga01         LIKE oga_file.oga01
  DEFINE l_i             LIKE type_file.num10
  DEFINE l_slip          LIKE aba_file.aba00

    CALL g_oga_l.clear()
    LET l_i = 1
    FOREACH t600_fill_cs INTO l_oga01
       IF SQLCA.sqlcode THEN
          CALL cl_err('foreach item_cur',SQLCA.sqlcode,1)
          CONTINUE FOREACH
       END IF
       SELECT oga00,oga08,oga01,oga02,oga011,oga16,oga03,oga032,oga14,gen02,
              oga15,gem02,oga55,ogaconf,ogapost
         INTO g_oga_l[l_i].*
         FROM oga_file
              LEFT OUTER JOIN gen_file ON oga14 = gen01
              LEFT OUTER JOIN gem_file ON oga15 = gem01
        WHERE oga01=l_oga01
       LET l_i = l_i + 1
       IF l_i > g_max_rec THEN
          IF g_action_choice ="query"  THEN
            CALL cl_err( '', 9035, 0 )
          END IF
          EXIT FOREACH
       END IF
    END FOREACH
    LET g_rec_b8 = l_i - 1
    DISPLAY ARRAY g_oga_l TO s_oga_l.* ATTRIBUTE(COUNT=g_rec_b8,UNBUFFERED)
       BEFORE DISPLAY
          EXIT DISPLAY
    END DISPLAY

END FUNCTION
