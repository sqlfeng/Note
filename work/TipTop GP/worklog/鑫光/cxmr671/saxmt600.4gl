                               l_ogg[i].ogg12, l_ogg[i].ogg15,
                               l_ogg[i].ogg15_fac,l_ogg[i].ogg16,
                               l_ogg[i].ogg20,l_ogg[i].ogg18,
                               g_plant,g_legal,l_ogg[i].ogg13) #FUN-C50097 ,l_ogg[i].ogg13
          IF SQLCA.sqlcode THEN
             CALL s_errmsg("ogg01",g_oga.oga01,"INS ogg_file",SQLCA.sqlcode,1)
             LET g_success = 'N'
             CONTINUE FOR
          END IF

          INITIALIZE r_ogc.* TO NULL
          LET r_ogc.ogc01 =g_oga.oga01
          LET r_ogc.ogc03 =g_ogb[l_ac].ogb03
          LET r_ogc.ogc09 =l_ogg[i].ogg09
          LET r_ogc.ogc091=l_ogg[i].ogg091
          LET r_ogc.ogc092=l_ogg[i].ogg092
          LET r_ogc.ogc18 =l_ogg[i].ogg18
          LET r_ogc.ogc17 =l_ogg[i].ogg17
          SELECT img09 INTO l_img09 FROM img_file
           WHERE img01=r_ogc.ogc17
             AND img02=r_ogc.ogc09
             AND img03=r_ogc.ogc091
             AND img04=r_ogc.ogc092
          IF l_ima906 = '1' THEN
             LET g_factor = 1
             LET r_ogc.ogc12=l_ogg[i].ogg12
             LET r_ogc.ogc13=l_ogg[i].ogg13 #FUN-C50097
