         #END IF
         #UPDATE x SET oao01 = g_oga.oga01
         #DELETE FROM oao_file WHERE oao01 = g_oga.oga01 AND oao03 = 0
         #INSERT INTO oao_file SELECT * FROM x
         #CHI-E30016 mark end---------------------------------------------------
