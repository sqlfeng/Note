FUNCTION t600_chk_ima928()
DEFINE l_cnt    LIKE type_file.num5
DEFINE l_ogb04  LIKE ogb_file.ogb04
DEFINE l_ima928 LIKE ima_file.ima928

   LET g_buf = s_get_doc_no(g_oga.oga01)
   LET l_cnt = 0
   SELECT COUNT(*) INTO l_cnt FROM gfa_file
    WHERE gfa01 = '2'
      AND gfa02 = g_buf
   IF l_cnt = 0 THEN LET g_success = 'N' RETURN END IF

   DECLARE t600_chk_ogb04 CURSOR FOR
    SELECT ogb04 FROM ogb_file
     WHERE ogb01 = g_oga.oga01

   FOREACH t600_chk_ogb04 INTO l_ogb04
      SELECT ima928 INTO l_ima928 FROM ima_file
       WHERE ima01 = l_ogb04
      IF l_ima928 = 'Y' THEN
         LET g_success = 'N'
         CALL cl_err(g_oga.oga01,'axm1190',1)
         EXIT FOREACH
      END IF
   END FOREACH
END FUNCTION

FUNCTION t600_chk_ima929_qty(p_cmd)
DEFINE l_ima929         LIKE ima_file.ima929
DEFINE l_ogb12_m        LIKE ogb_file.ogb12   #記錄出通單母料的數量
DEFINE l_ogb12_c        LIKE ogb_file.ogb12   #記錄出通單子料的數量
DEFINE l_ogb12_c_sum    LIKE ogb_file.ogb12   #記錄子料在出貨單的總量
DEFINE l_ogb04          LIKE ogb_file.ogb04   #記錄USER出貨時,可能挑選的特性子料
DEFINE p_cmd            LIKE type_file.chr1

   #撈取該子料對應的特性母料
   SELECT ima929 INTO l_ima929 FROM ima_file
    WHERE ima01 = g_ogb[l_ac].ogb04

   IF NOT cl_null(l_ima929) THEN

      LET g_ogb[l_ac].ogb12 = 0
      LET g_sql = "SELECT ima01 FROM ima_file ",
                  " WHERE ima929 = '",l_ima929,"'"
      PREPARE pre_sel_ima01 FROM g_sql
      DECLARE cur_ima01 CURSOR FOR pre_sel_ima01
      FOREACH cur_ima01 INTO l_ogb04   #撈取可能挑選到的特性子料
         #撈取出通單該特性子料的量
         LET l_ogb12_c = 0
         SELECT ogb12 INTO l_ogb12_c FROM ogb_file,oga_file
         WHERE ogb01 = g_oga.oga011
         AND ogb04 = l_ogb04 #特性子料
         AND oga01 = ogb01
         AND ogaconf <> 'X'
         IF cl_null(l_ogb12_c) THEN LET l_ogb12_c = 0 END IF

         #計算該特性子料已產生出貨單的量
         LET l_ogb12_c_sum = 0
         SELECT SUM(ogb12) INTO l_ogb12_c_sum FROM ogb_file,oga_file
          WHERE ogb01 = g_oga.oga01
            AND ogb04 = l_ogb04 #特性子料
            AND ogb03 <> g_ogb[l_ac].ogb03
            AND oga01 = ogb01
            AND ogaconf <> 'X'
         IF cl_null(l_ogb12_c_sum) THEN LET l_ogb12_c_sum = 0 END IF

         #最大可出數量=出通單子料-出貨單子料已出量+[最後加上(出通單母料量)]
          LET g_ogb[l_ac].ogb12 = g_ogb[l_ac].ogb12 + l_ogb12_c - l_ogb12_c_sum
      END FOREACH

      #撈取母料出通單單身維護的數量
      LET l_ogb12_m = 0
      SELECT ogb12 INTO l_ogb12_m FROM ogb_file,oga_file
       WHERE ogb01 = g_oga.oga011
         AND ogb04 = l_ima929 #特性母料
         AND oga01 = ogb01
         AND ogaconf <> 'X'
      IF cl_null(l_ogb12_m) THEN LET l_ogb12_m = 0 END IF

      #最後加上(出通單母料量)
      LET g_ogb[l_ac].ogb12 = g_ogb[l_ac].ogb12 + l_ogb12_m

      #單身挑選的特性子料於出通單的量
      LET l_ogb12_c = 0
      SELECT ogb12 INTO l_ogb12_c FROM ogb_file,oga_file
       WHERE ogb01 = g_oga.oga011
         AND ogb04 = g_ogb[l_ac].ogb04 #單身挑選的特性子料
         AND oga01 = ogb01
         AND ogaconf <> 'X'
      IF cl_null(l_ogb12_c) THEN LET l_ogb12_c = 0 END IF

      #計算單身挑選的特性子料已產生出貨單的量
      LET l_ogb12_c_sum = 0
      SELECT SUM(ogb12) INTO l_ogb12_c_sum FROM ogb_file,oga_file
       WHERE ogb01 = g_oga.oga01
         AND ogb04 = g_ogb[l_ac].ogb04 #單身挑選的特性子料
         AND ogb03 <> g_ogb[l_ac].ogb03
         AND oga01 = ogb01
         AND ogaconf <> 'X'
      IF cl_null(l_ogb12_c_sum) THEN LET l_ogb12_c_sum = 0 END IF

      #出貨量不可大於出通單(母料+子料)的量
      IF (g_ogb[l_ac].ogb12 > l_ogb12_c + l_ogb12_m - l_ogb12_c_sum ) THEN
         LET g_ogb[l_ac].ogb12 = l_ogb12_c + l_ogb12_m - l_ogb12_c_sum
      END IF
      IF g_ogb[l_ac].ogb12 < 0 THEN LET g_ogb[l_ac].ogb12 = 0 END IF
      LET g_ogb[l_ac].ogb912 = g_ogb[l_ac].ogb12
      LET g_ogb[l_ac].ogb915 = 0
      CALL t600_set_ogb917()
      IF p_cmd = 'u' THEN
         UPDATE ogb_file SET ogb12=g_ogb[l_ac].ogb12
          WHERE ogb01=g_oga.oga01
            AND ogb03=g_ogb[l_ac].ogb03
      END IF
   END IF

END FUNCTION

FUNCTION t600_qry_ima929(p_cmd)
DEFINE l_oeb04         LIKE oeb_file.oeb04
DEFINE l_ima928        LIKE ima_file.ima928
DEFINE l_ogb04_o       LIKE ogb_file.ogb04
DEFINE l_chose         LIKE type_file.num5
DEFINE p_cmd           LIKE type_file.chr1

   SELECT oeb04 INTO l_oeb04 FROM oeb_file
     WHERE oeb01=g_ogb[l_ac].ogb31 AND oeb03=g_ogb[l_ac].ogb32
   SELECT ima928 INTO l_ima928 FROM ima_file
    WHERE ima01=l_oeb04
   IF l_ima928='Y' THEN
      SELECT COUNT(*) INTO l_chose FROM oeba_file,imac_file
       WHERE oeba01 = g_ogb[l_ac].ogb31
         AND oeba02 = g_ogb[l_ac].ogb32
         AND imac01 = l_oeb04
         AND oeba04 = imac04
         AND imac03 = '1'
         AND (oeba05 IS NOT NULL OR oeba06 IS NOT NULL)
      LET l_ogb04_o = g_ogb[l_ac].ogb04
      IF l_chose = 0 THEN
         CALL cl_init_qry_var()
         LET g_qryparam.form = "q_ima"
         LET g_qryparam.where = "ima929 = '",l_oeb04,"'"
         CALL cl_create_qry() RETURNING g_ogb[l_ac].ogb04
      ELSE
         CALL q_ima929(FALSE,FALSE,g_ogb[l_ac].ogb31,g_ogb[l_ac].ogb32,l_oeb04) RETURNING g_ogb[l_ac].ogb04
      END IF
      IF g_ogb[l_ac].ogb04 IS NULL THEN
         LET g_ogb[l_ac].ogb04 = l_ogb04_o
        #NEXT FIELD ogb32
      END IF
      SELECT ima02,ima021 INTO g_ogb[l_ac].ogb06,g_ogb[l_ac].ima021
        FROM ima_file WHERE ima01=g_ogb[l_ac].ogb04
      UPDATE ogb_file SET ogb04=g_ogb[l_ac].ogb04,ogb06=g_ogb[l_ac].ogb06
                    WHERE ogb01=g_oga.oga01
                      AND ogb03=g_ogb[l_ac].ogb03
      IF NOT cl_null(g_oga.oga011) THEN
         CALL t600_chk_ima929_qty(p_cmd)
      END IF
   END IF
END FUNCTION
