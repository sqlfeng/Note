      IF NOT cl_null(g_ogb[l_ac].ogb04) THEN
         IF (g_ogb_t.ogb916 IS NULL OR g_ogb_t.ogb916 <> g_ogb[l_ac].ogb916) AND g_aza.aza50 ='Y' THEN    #No.FUN-670008
