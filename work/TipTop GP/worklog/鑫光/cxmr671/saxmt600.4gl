             END IF
             LET l_cnt = 0
             SELECT COUNT(*) INTO l_cnt
               FROM npp_file
              WHERE nppsys = 'AR'
                AND npp00 = 1
                AND npp01 = g_oga.oga01
                AND npp011 = 1
             IF l_cnt = 0 AND g_oga.oga09 = '2' AND g_oga.oga07 = 'Y' THEN
                CALL s_t600_gl(g_oga.oga01,'0')
                IF g_aza.aza63 = 'Y' THEN
                   CALL s_t600_gl(g_oga.oga01,'1')
                END IF

                LET g_success = 'Y'
