         IF g_oga.oga09 MATCHES '[2468]' AND g_oga.oga01 != g_oga.oga011 THEN
            LET l_oao01=g_oga.oga011
         ELSE
            LET l_oao01=g_oga.oga16
         END IF
         DECLARE t600_oao_1 CURSOR FOR
            SELECT * FROM oao_file
             WHERE oao01=l_oao01
               AND oao03=0
         FOREACH t600_oao_1 INTO l_oao.*
            LET l_oao.oao01=g_oga.oga01
            INSERT INTO oao_file VALUES(l_oao.*)
            IF SQLCA.sqlcode THEN
               CALL cl_err3("ins","oao_file",g_oga.oga01,"",SQLCA.sqlcode,"","ins oao",1)
               ROLLBACK WORK
               EXIT WHILE
            END IF
         END FOREACH
         #CHI-E30016 add end----------------------------------------------------
         CALL t600_show_oao()
         EXIT WHILE
      END WHILE
   END IF

   IF l_flag = 'b' THEN
      WHILE TRUE
         LET l_cnt = 0
         SELECT COUNT(*) INTO l_cnt FROM oao_file
          WHERE oao01 = g_oga.oga01
            AND oao03 = g_ogb[l_ac].ogb03
         IF cl_null(l_cnt) THEN LET l_cnt = 0 END IF
         IF l_cnt > 0 THEN
           #IF NOT cl_confirm('axm_113') THEN   #MOD-B70173 mark
            IF NOT cl_confirm('axm-624') THEN   #MOD-B70173
               EXIT WHILE
            END IF
         END IF
         #CHI-E30016 mark-------------------------------------------------------
         #DROP TABLE x
         #DELETE FROM x
         #SELECT * FROM oao_file
         # WHERE oao01=g_ogb[l_ac].ogb31
         #   AND oao03=g_ogb[l_ac].ogb32
         #  INTO TEMP x
         #UPDATE x SET oao01 = g_oga.oga01,
         #             oao03 = g_ogb[l_ac].ogb03
         #DELETE FROM oao_file WHERE oao01 = g_oga.oga01 AND oao03 = g_ogb[l_ac].ogb03
         #INSERT INTO oao_file SELECT * FROM x
         #CHI-E30016 mark end---------------------------------------------------
         #CHI-E30016 add--------------------------------------------------------
         DECLARE t600_oao_2 CURSOR FOR
          SELECT * FROM oao_file
           WHERE oao01=g_ogb[l_ac].ogb31
             AND oao03=g_ogb[l_ac].ogb32
         FOREACH t600_oao_2 INTO l_oao.*
            LET l_oao.oao01=g_oga.oga01
            LET l_oao.oao03=g_ogb[l_ac].ogb03
            INSERT INTO oao_file VALUES(l_oao.*)
            IF SQLCA.sqlcode THEN
               CALL cl_err3("ins","oao_file",g_oga.oga01,"",SQLCA.sqlcode,"","ins oao",1)
               ROLLBACK WORK
               EXIT WHILE
            END IF
         END FOREACH
         #CHI-E30016 add end----------------------------------------------------
         EXIT WHILE
      END WHILE
   END IF

   IF l_flag = 'o' THEN
      DECLARE t600_ogb31_ogb32
        CURSOR FOR SELECT ogb03,ogb31,ogb32 FROM ogb_file
                    WHERE ogb01 = g_oga.oga01
      FOREACH t600_ogb31_ogb32 INTO l_ogb03,l_ogb31,l_ogb32
         #CHI-E30016 mark-------------------------------------------------------
         #DROP TABLE x
         #SELECT * FROM oao_file
         # WHERE oao01=l_ogb31
         #   AND oao03=l_ogb32
         #  INTO TEMP x
         #UPDATE x SET oao01 = g_oga.oga01,
         #             oao03 = l_ogb03
         #DELETE FROM oao_file WHERE oao01 = g_oga.oga01 AND oao03 = l_ogb03
         #INSERT INTO oao_file SELECT * FROM x
         #CHI-E30016 mark end---------------------------------------------------
         #CHI-E30016 add--------------------------------------------------------
         DECLARE t600_oao_3 CURSOR FOR
          SELECT * FROM oao_file
           WHERE oao01=l_ogb31
             AND oao03=l_ogb32
         FOREACH t600_oao_3 INTO l_oao.*
