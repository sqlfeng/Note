           #CALL t600_price(l_ac) RETURNING l_fac       #FUN-AC0012
               CALL t600_price(p_cmd,l_ac) RETURNING l_fac #FUN-AC0012
            END IF                                                               #MOD-B60052
         END IF
      END IF
   END IF
   CALL t600_set_required_b1('')
   RETURN NULL
END FUNCTION

