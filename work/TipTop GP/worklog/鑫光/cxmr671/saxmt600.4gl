                              g_plant,g_legal,l_ogc[i].ogc13) #FUN-C50097 ADD ogc13 ,l_ogc[i].ogc13 TQC-C70206
         IF SQLCA.sqlcode THEN
            CALL cl_err3("ins","ogc_file","","",SQLCA.sqlcode,"","INS-ogc",1)  #No.FUN-670008
            LET g_success = 'N'
            EXIT FOR
         END IF
      END FOR

      #MOD-C30841 add start -----
      DECLARE t600_del_ogc_1 CURSOR FOR SELECT rvbs02,rvbs13 FROM rvbs_file
                                       WHERE rvbs00 = g_prog AND rvbs01 = g_oga.oga01
                                         AND rvbs02 = g_ogb[l_ac].ogb03
      FOREACH t600_del_ogc_1 INTO l_rvbs02,l_rvbs13
         SELECT COUNT(*) INTO l_cnt FROM ogc_file WHERE ogc01 = g_oga.oga01 AND ogc03 = l_rvbs02 AND ogc18 = l_rvbs13
         IF l_cnt = 0 THEN
