                  "   AND ((oeb12-oeb24+oeb25)>0 OR oeb1003 ='2')",
                  "   AND oeb70='N'",
                 #"   AND oeb04=ima_file.ima01",             #TQC-A60132
                  "   AND oeb_file.oeb04=ima_file.ima01",    #TQC-A60132
                  " ORDER BY oeb03"
   END IF

   PREPARE t600_g_b_pb FROM g_sql
