            SELECT ima929 INTO l_ima929 FROM ima_file
             WHERE ima01 = g_ogb[l_ac].ogb04
            IF NOT cl_null(l_ima929) THEN

               #計算該特性子料已產生出貨單的量
               SELECT SUM(ogb12) INTO l_ogb12_c_sum FROM ogb_file,oga_file
                WHERE ogb01 = g_oga.oga01
                  AND ogb04 = g_ogb[l_ac].ogb04
                  AND oga01 = ogb01
                  AND ogb03 <> g_ogb[l_ac].ogb03
                  AND ogaconf != 'X'
               IF cl_null(l_ogb12_c_sum) THEN LET l_ogb12_c_sum = 0 END IF

               #撈取母料出通單單身維護的數量
               SELECT ogb12 INTO l_ogb12_m FROM ogb_file,oga_file
                WHERE ogb01 = g_oga.oga011
                  AND ogb04 = l_ima929 #特性母料
                  AND oga01 = ogb01
                  AND ogaconf != 'X'
               IF cl_null(l_ogb12_m) THEN LET l_ogb12_m = 0 END IF

               SELECT ogb12 INTO l_ogb12_c FROM ogb_file,oga_file
                WHERE ogb01 = g_oga.oga011
                  AND ogb04 = g_ogb[l_ac].ogb04 #特性子料
                  AND oga01 = ogb01
                  AND ogaconf != 'X'
               IF cl_null(l_ogb12_c) THEN LET l_ogb12_c = 0 END IF
               IF g_ogb[l_ac].ogb12 > l_ogb12_m + l_ogb12_c - l_ogb12_c_sum THEN
