              SELECT ogc17,ogc09,ogc091,ogc092,ogc12,ogc13,img10,ogc15,ogc15_fac,ogc16,ogc18  #FUN-8A0030 Add   #CHI-9C0024 move ogc12
                FROM ogc_file, OUTER img_file  #FUN-C50097 ADD OGC13 ogc13,
               WHERE ogc01 = g_oga.oga01 AND ogc03 = g_ogb[l_ac].ogb03
                 AND img01=ogc17
                 AND img02=ogc09
                 AND img03=ogc091
                 AND img04=ogc092

      CALL l_ogc.clear()
