FUNCTION t600_mlog(p_cmd)       # Transaction Modify Log (存入 oem_file)
   DEFINE p_cmd         LIKE type_file.chr1    #No.FUN-680137 VARCHAR(1)
   DEFINE l_oem05       LIKE type_file.chr8    #No.FUN-680137 VARCHAR(8)
   DEFINE l_oem         RECORD LIKE oem_file.*
   IF g_oga.oga06 IS NULL THEN RETURN END IF
   INITIALIZE l_oem.* TO NULL
   LET l_oem.oem01=g_oga.oga01
   LET l_oem.oem03=b_ogb.ogb03
   LET l_oem.oem04=g_today
   LET l_oem05 = TIME
   LET l_oem.oem05=l_oem05
   LET l_oem.oem06=g_user
   LET l_oem.oem07=p_cmd
   LET l_oem.oem08=g_oga.oga06
   IF p_cmd='A' THEN
      LET l_oem.oem10o=NULL LET l_oem.oem11o=NULL
      LET l_oem.oem12o=NULL LET l_oem.oem13o=NULL LET l_oem.oem15o=NULL
      LET l_oem.oem12n=b_ogb.ogb12
   END IF
   IF p_cmd='R' THEN
      LET l_oem.oem10o=g_ogb_t.ogb04
      LET l_oem.oem11o=g_ogb_t.ogb092
      LET l_oem.oem12o=g_ogb_t.ogb12
      LET l_oem.oem10n=NULL LET l_oem.oem11n=NULL
      LET l_oem.oem12n=NULL LET l_oem.oem13n=NULL LET l_oem.oem15n=NULL
   END IF
   IF p_cmd='U' THEN
      IF g_ogb_t.ogb04 != b_ogb.ogb04 THEN
         LET l_oem.oem10o=g_ogb_t.ogb04 LET l_oem.oem10n=b_ogb.ogb04
      END IF
      IF g_ogb_t.ogb092 != b_ogb.ogb092 THEN
         LET l_oem.oem11o=g_ogb_t.ogb092 LET l_oem.oem11n=b_ogb.ogb092
      END IF
      IF g_ogb_t.ogb12 != b_ogb.ogb12 THEN
         LET l_oem.oem12o=g_ogb_t.ogb12 LET l_oem.oem12n=b_ogb.ogb12
      END IF
      IF cl_null(l_oem.oem10n) AND cl_null(l_oem.oem11n) AND
         cl_null(l_oem.oem12n) AND cl_null(l_oem.oem13n) AND
         cl_null(l_oem.oem15n) THEN RETURN
      END IF
      IF l_oem.oem12n=0 THEN LET l_oem.oem12n=NULL END IF
      IF l_oem.oem13n=0 THEN LET l_oem.oem13n=NULL END IF
   END IF

   LET l_oem.oemplant = g_plant
   LET l_oem.oemlegal = g_legal

   INSERT INTO oem_file VALUES (l_oem.*)
END FUNCTION

FUNCTION t600_bu()
   DEFINE l_oea61   LIKE oea_file.oea61    #No:FUN-A50103
   DEFINE l_oea1008 LIKE oea_file.oea1008  #No:FUN-A50103
   DEFINE l_oea261  LIKE oea_file.oea261   #No:FUN-A50103
   DEFINE l_oea262  LIKE oea_file.oea262   #No:FUN-A50103
   DEFINE l_oea263  LIKE oea_file.oea263   #No:FUN-A50103
   DEFINE l_len     LIKE type_file.num5    #No:MOD-B70185
   DEFINE l_m       LIKE type_file.num5    #No:MOD-B70185
   DEFINE l_oga52   LIKE oga_file.oga52    #No:MOD-B70185
   DEFINE l_oga53   LIKE oga_file.oga53    #No:MOD-B70185
   DEFINE l_sql     STRING                 #MOD-C70208 add
   DEFINE l_ogb14   LIKE ogb_file.ogb14    #MOD-C70208 add
   DEFINE l_ogb31   LIKE ogb_file.ogb31    #MOD-C70208 add

   LET g_oga.oga50 = NULL

   SELECT SUM(ogb14) INTO g_oga.oga50 FROM ogb_file WHERE ogb01 = g_oga.oga01
   SELECT SUM(ogb14t) INTO g_oga.oga51 FROM ogb_file WHERE ogb01 = g_oga.oga01        #TQC-BB0018 add

   CALL cl_digcut(g_oga.oga50,t_azi04) RETURNING g_oga.oga50  #CHI-7A0036-add
   IF cl_null(g_oga.oga50) THEN LET g_oga.oga50 = 0 END IF

   IF NOT cl_null(g_oga.oga16) THEN   #MOD-B70185
      #-----No:FUN-A50103-----
      SELECT oea61,oea1008,oea261,oea262,oea263
        INTO l_oea61,l_oea1008,l_oea261,l_oea262,l_oea263
        FROM oea_file
       WHERE oea01 = g_oga.oga16
      IF g_oga.oga213 = 'Y' THEN
         LET g_oga.oga52 = g_oga.oga50 * l_oea261 / l_oea1008
        #LET g_oga.oga53 = g_oga.oga50 * (l_oea262+l_oea263) / l_oea1008   #MOD-D70038 mark
         LET g_oga.oga53 = g_oga.oga50 * l_oea262 / l_oea1008              #MOD-D70038 add
      ELSE
         LET g_oga.oga52 = g_oga.oga50 * l_oea261 / l_oea61
        #LET g_oga.oga53 = g_oga.oga50 * (l_oea262+l_oea263) / l_oea61     #MOD-D70038 mark
         LET g_oga.oga53 = g_oga.oga50 * l_oea262 / l_oea61                #MOD-D70038 add
      END IF
   #MOD-B70185 add begin------------
   ELSE
     #LET l_len = g_ogb.getLength() #MOD-C70208 mark
      LET l_oga52 = 0
      LET l_oga53 = 0
     #MOD-C70208 mark start -----
     #FOR l_m=1 TO l_len
     #   IF NOT cl_null(g_ogb[l_m].ogb31) THEN
     #MOD-C70208 mark end   -----

      #MOD-C70208 add start -----
      LET l_sql = "SELECT ogb14, ogb31 FROM ogb_file WHERE ogb01 = ?"
      PREPARE sel_ogb_bu FROM l_sql
      DECLARE cur_ogb_bu CURSOR FOR sel_ogb_bu
      FOREACH cur_ogb_bu USING g_oga.oga01 INTO l_ogb14,l_ogb31
         IF NOT cl_null(l_ogb31) THEN
      #MOD-C70208 add end   -----
            SELECT oea61,oea1008,oea261,oea262,oea263
              INTO l_oea61,l_oea1008,l_oea261,l_oea262,l_oea263
              FROM oea_file
            #WHERE oea01 = g_ogb[l_m].ogb31  #MOD-C70208 mark
             WHERE oea01 = l_ogb31           #MOD-C70208 add
            IF g_oga.oga213 = 'Y' THEN
              #LET l_oga52 = l_oga52 + g_ogb[l_m].ogb14 * l_oea261 / l_oea1008               #MOD-C70208 mark
              #LET l_oga53 = l_oga53 + g_ogb[l_m].ogb14 * (l_oea262+l_oea263) / l_oea1008    #MOD-C70208 mark
               LET l_oga52 = l_oga52 + l_ogb14 * l_oea261 / l_oea1008                      #MOD-C70208 add
              #LET l_oga53 = l_oga53 + l_ogb14 * (l_oea262+l_oea263) / l_oea1008           #MOD-C70208 add   #MOD-D70038 mark
               LET l_oga53 = l_oga53 + l_ogb14 * l_oea262 / l_oea1008                                        #MOD-D70038 add
            ELSE
              #LET l_oga52 = l_oga52 + g_ogb[l_m].ogb14 * l_oea261 / l_oea61                 #MOD-C70208 mark
              #LET l_oga53 = l_oga53 + g_ogb[l_m].ogb14 * (l_oea262+l_oea263) / l_oea61      #MOD-C70208 mark
               LET l_oga52 = l_oga52 + l_ogb14 * l_oea261 / l_oea61                        #MOD-C70208 add
              #LET l_oga53 = l_oga53 + l_ogb14 * (l_oea262+l_oea263) / l_oea61             #MOD-C70208 add   #MOD-D70038 mark
               LET l_oga53 = l_oga53 + l_ogb14 * l_oea262 / l_oea61                                          #MOD-D70038 add
            END IF
         ELSE #无订单号码
           #LET l_oga52 = l_oga52 + g_ogb[l_m].ogb14 * g_oga.oga161/100                 #MOD-C70208 mark
           #LET l_oga53 = l_oga53 + g_ogb[l_m].ogb14 * (g_oga.oga162+g_oga.oga163)/100  #MOD-C70208 mark
            LET l_oga52 = l_oga52 + l_ogb14 * g_oga.oga161 / 100                    #MOD-C70208 add
            LET l_oga53 = l_oga53 + l_ogb14 * (g_oga.oga162+g_oga.oga163) / 100     #MOD-C70208 add
         END IF
      END FOREACH #MOD-C70208 add
     #END FOR     #MOD-C70208 mark
      LET g_oga.oga52 = l_oga52
      LET g_oga.oga53 = l_oga53
   END IF

   #MOD-B70185 add end -------------
  #LET g_oga.oga52 = g_oga.oga50 * g_oga.oga161/100
  #LET g_oga.oga53 = g_oga.oga50 * (g_oga.oga162+g_oga.oga163)/100
   #-----No:FUN-A50103 END-----
   CALL cl_digcut(g_oga.oga52,t_azi04) RETURNING g_oga.oga52  #CHI-7A0036-add
   CALL cl_digcut(g_oga.oga53,t_azi04) RETURNING g_oga.oga53  #CHI-7A0036-add
#TQC-A70061 --add
 IF cl_null(g_oga.oga52) THEN LET g_oga.oga52 = 0 END IF
 IF cl_null(g_oga.oga53) THEN LET g_oga.oga53 = 0 END IF
#TQC-A70061 --end
   UPDATE oga_file SET oga50=g_oga.oga50,
                       oga51=g_oga.oga51,               #TQC-BB0018 add
                       oga52=g_oga.oga52,
                       oga53=g_oga.oga53
    WHERE oga01 = g_oga.oga01
   IF STATUS THEN
      CALL cl_err3("upd","oga_file",g_oga.oga01,"",SQLCA.sqlcode,"","_bu():upd oga",1)  #No.FUN-670008
   END IF

END FUNCTION

