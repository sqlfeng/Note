           END IF


        BEFORE FIELD oga11
              IF NOT cl_null(g_oga.oga32) THEN
                 SELECT oag02 INTO g_buf FROM oag_file
                  WHERE oag01=g_oga.oga32
                 IF STATUS THEN
                    CALL cl_err3("sel","oag_file",g_oga.oga32,"",SQLCA.sqlcode,"","select oag",1)  #No.FUN-670008
