   IF g_wc_tmp <> ' 1=1' THEN
      LET g_sql = g_sql,
                  " AND EXISTS(SELECT 1 ",
                  "              FROM t600_otn_tmp ",
                  "             WHERE ta_ogb01_1 = ta_ogb01 ",
                  "               AND ",g_wc_tmp CLIPPED,") "
   END IF
