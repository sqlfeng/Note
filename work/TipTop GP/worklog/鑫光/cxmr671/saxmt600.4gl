          INSERT INTO ogg_file(ogg17,ogg01,ogg03,ogg09,ogg091,ogg092,ogg10,
                               ogg12,ogg15,ogg15_fac,ogg16,ogg20,ogg18,
                               oggplant,ogglegal,ogg13) #,ogg13
                        VALUES(l_ogg[i].ogg17,g_oga.oga01,g_ogb[l_ac].ogb03,
