FUNCTION t600_sal_b()
DEFINE l_cnt           LIKE type_file.num5
DEFINE l_ac7_t         LIKE type_file.num5
DEFINE l_n             LIKE type_file.num5
DEFINE l_lock_sw       LIKE type_file.chr1
DEFINE p_cmd           LIKE type_file.chr1
DEFINE l_start         LIKE rxe_file.rxe04
DEFINE l_end           LIKE rxe_file.rxe05
DEFINE l_lpx22         LIKE lpx_file.lpx22
DEFINE l_rxe03         LIKE rxe_file.rxe03
DEFINE l_ima154        LIKE ima_file.ima154
DEFINE l_rxe08_sum     LIKE rxe_file.rxe08
DEFINE l_allow_insert  LIKE type_file.chr1
DEFINE l_allow_delete  LIKE type_file.chr1


    IF s_shut(0) THEN
       RETURN
    END IF

    SELECT ima154 INTO l_ima154
      FROM ima_file
     WHERE ima01 = g_ogb[l_ac].ogb04
    IF l_ima154 = 'N' OR g_flag3 = '1' THEN
       LET g_action_choice = "exit"
       RETURN
    END IF
    CALL cl_opmsg('b')
    LET g_action_choice = ""

    LET g_forupd_sql = "SELECT rxe02,rxe03,rxe04,rxe05,rxe06,'',rxe07,'',rxe08,rxe09 ",
                       "  FROM rxe_file WHERE rxe00= '02' ",
                       "   AND rxe01 = ? AND rxe02 = ? AND rxe03 = ? FOR UPDATE "
    LET g_forupd_sql=cl_forupd_sql(g_forupd_sql)

    DECLARE t600_bcl CURSOR FROM g_forupd_sql

    LET l_allow_insert = cl_detail_input_auth('insert')
    LET l_allow_delete = cl_detail_input_auth('delete')

    INPUT ARRAY g_rxe WITHOUT DEFAULTS FROM s_rxe.*
          ATTRIBUTE (COUNT=g_rec_b7,MAXCOUNT=g_max_rec,UNBUFFERED,
                     INSERT ROW = l_allow_insert,DELETE ROW=l_allow_delete,APPEND ROW=l_allow_insert)

       BEFORE INPUT
          IF g_rec_b7 != 0 THEN
             CALL fgl_set_arr_curr(l_ac7)
             LET l_ac7 = 1
          END IF
       BEFORE ROW
          LET p_cmd=''
          LET l_ac7 = ARR_CURR()
          LET l_lock_sw = 'N'
          LET l_n  = ARR_COUNT()
          BEGIN WORK

          IF g_rec_b7>=l_ac7 THEN
             LET p_cmd='u'

             LET g_rxe_t.* = g_rxe[l_ac7].*
             OPEN t600_bcl USING g_oga.oga01,g_rxe_t.rxe02,g_rxe_t.rxe03
             IF STATUS THEN
                CALL s_errmsg('','','OPEN t600_bcl:',STATUS,1)
                LET l_lock_sw = "Y"
             ELSE
                FETCH t600_bcl INTO g_rxe[l_ac7].*
                IF SQLCA.sqlcode THEN
                   CALL s_errmsg('',g_rxe_t.rxe02,'',SQLCA.sqlcode,1)
                   CALL cl_err(g_rxe_t.rxe02,SQLCA.sqlcode,1)
                   LET l_lock_sw = "Y"
                END IF
                SELECT lpx02 INTO g_rxe[l_ac7].lpx02
                  FROM lqe_file
                 INNER JOIN lpx_file
                    ON lqe02 = lpx01
                 WHERE lqe01 = g_rxe[l_ac7].rxe04

                SELECT lrz02 INTO g_rxe[l_ac7].lrz02
                  FROM lqe_file
                 INNER JOIN lrz_file
                    ON lqe03 = lrz01
                 WHERE lqe01 = g_rxe[l_ac7].rxe04
             END IF
          END IF

       BEFORE INSERT
          LET l_n=ARR_COUNT()
          LET p_cmd='a'
          INITIALIZE g_rxe[l_ac7].* TO NULL
          LET g_rxe[l_ac7].rxe02 = g_ogb[l_ac].ogb03
          CALL cl_set_comp_entry("rxe02",FALSE)
          LET g_rxe_t.*=g_rxe[l_ac7].*
          CALL cl_show_fld_cont()
          NEXT FIELD rxe03

       AFTER INSERT
          IF INT_FLAG THEN
             CALL cl_err('',9001,0)
             LET INT_FLAG=0
             CANCEL INSERT
          END IF
          INSERT INTO rxe_file(rxe00,rxe01,rxe02,rxe03,rxe04,rxe05,rxe06,
                               rxe07,rxe08,rxe09,rxeplant,rxelegal)
                        VALUES('02',g_oga.oga01,g_rxe[l_ac7].rxe02,g_rxe[l_ac7].rxe03,g_rxe[l_ac7].rxe04,
                               g_rxe[l_ac7].rxe05,g_rxe[l_ac7].rxe06,g_rxe[l_ac7].rxe07,g_rxe[l_ac7].rxe08,
                               g_rxe[l_ac7].rxe09,g_oga.ogaplant,g_oga.ogalegal)
          IF SQLCA.sqlcode THEN
             CALL cl_err3("ins","rxe_file",g_oga.oga01,g_rxe[l_ac7].rxe02,SQLCA.sqlcode,"","",1)
             CANCEL INSERT
          ELSE
             MESSAGE 'INSERT O.K.'
             LET g_sal = 'Y'
             COMMIT WORK
             LET g_rec_b7=g_rec_b7+1
             DISPLAY g_rec_b7 TO FORMONLY.cnt
          END IF

       BEFORE FIELD rxe03
           IF g_rxe[l_ac7].rxe03 IS NULL OR g_rxe[l_ac7].rxe03 = 0 THEN
              SELECT max(rxe03) INTO l_rxe03
                FROM rxe_file
               WHERE rxe01 = g_oga.oga01
                 AND rxe02 = g_rxe[l_ac7].rxe02
             IF cl_null(l_rxe03) THEN
                LET g_rxe[l_ac7].rxe03 = 1
             ELSE
                LET g_rxe[l_ac7].rxe03 = l_rxe03 +1
             END IF
           END IF

       AFTER FIELD rxe03
          IF NOT cl_null(g_rxe[l_ac7].rxe03) THEN
              IF g_rxe[l_ac7].rxe03 != g_rxe_t.rxe03 OR g_rxe_t.rxe03 IS NULL THEN
                 IF g_rxe[l_ac7].rxe03 <= 0 THEN
                    CALL cl_err('','aec-994',0)
                    LET g_rxe[l_ac7].rxe03 = g_rxe_t.rxe03
                    NEXT FIELD rxe03
                 END IF
                 SELECT count(*)
                   INTO l_n
                   FROM rxe_file
                  WHERE rxe01 = g_oga.oga01
                    AND rxe02 = g_rxe[l_ac7].rxe02
                    AND rxe03 = g_rxe[l_ac7].rxe03
                 IF l_n > 0 THEN
                    CALL cl_err('','-239',0)
                    LET g_rxe[l_ac7].rxe03 = g_rxe_t.rxe03
                    NEXT FIELD rxe03
                 END IF
              END IF
           END IF
       AFTER FIELD rxe04  #起始编号
          IF NOT cl_null(g_rxe[l_ac7].rxe04) THEN
             CALL t600_rxe04_rxe05(g_rxe[l_ac7].rxe04,p_cmd)
             IF NOT cl_null(g_errno) THEN
                CALL cl_err('',g_errno,0)
                LET g_rxe[l_ac7].rxe04 = g_rxe_t.rxe04
                NEXT FIELD rxe04
             END IF
             IF g_success = 'N' THEN
                LET g_rxe[l_ac7].rxe04 = g_rxe_t.rxe04
                NEXT FIELD rxe04
             END IF
          END IF

       AFTER FIELD rxe05 #结束编号
          IF NOT cl_null(g_rxe[l_ac7].rxe05) THEN
             CALL t600_rxe04_rxe05(g_rxe[l_ac7].rxe05,p_cmd)
             IF NOT cl_null(g_errno) THEN
                CALL cl_err('',g_errno,0)
                LET g_rxe[l_ac7].rxe05 = g_rxe_t.rxe05
                NEXT FIELD rxe05
             END IF
             IF g_success = 'N' THEN
                LET g_rxe[l_ac7].rxe05 = g_rxe_t.rxe05
                NEXT FIELD rxe05
             END IF
          END IF


       BEFORE DELETE
           IF NOT cl_delb(0,0) THEN
              CANCEL DELETE
           END IF
           IF l_lock_sw = "Y" THEN
              CALL cl_err("", -263, 1)
              CANCEL DELETE
           END IF
           DELETE FROM rxe_file
            WHERE rxe00 = '02'
              AND rxe01 = g_oga.oga01
              AND rxe02 = g_rxe_t.rxe02
              AND rxe03 = g_rxe_t.rxe03
           IF SQLCA.sqlcode THEN
              CALL cl_err3("del","rxe_file",g_oga.oga01,g_rxe_t.rxe02,SQLCA.sqlcode,"","",1)
              ROLLBACK WORK
              CANCEL DELETE
           END IF
           LET g_rec_b7=g_rec_b7-1
           DISPLAY g_rec_b7 TO FORMONLY.cnt
           COMMIT WORK

       ON ROW CHANGE
          IF INT_FLAG THEN
             CALL cl_err('',9001,0)
             LET g_rxe[l_ac7].* = g_rxe_t.*
             CLOSE t600_bcl
             LET g_success = 'N'
             EXIT INPUT
           END IF
           IF l_lock_sw="Y" THEN
              CALL cl_err(g_rxe_t.rxe02,-263,0)
              LET g_rxe[l_ac7].* = g_rxe_t.*
           ELSE
              UPDATE rxe_file SET rxe02 = g_rxe[l_ac7].rxe02,
                                  rxe03 = g_rxe[l_ac7].rxe03,
                                  rxe04 = g_rxe[l_ac7].rxe04,
                                  rxe05 = g_rxe[l_ac7].rxe05,
                                  rxe06 = g_rxe[l_ac7].rxe06,
                                  rxe07 = g_rxe[l_ac7].rxe07,
                                  rxe08 = g_rxe[l_ac7].rxe08,
                                  rxe09 = g_rxe[l_ac7].rxe09
               WHERE rxe01 = g_oga.oga01
                 AND rxe00 = '02'
                 AND rxe02 = g_rxe_t.rxe02
                 AND rxe03 = g_rxe_t.rxe03
              IF SQLCA.sqlcode OR SQLCA.sqlerrd[3] = 0 THEN
                 CALL s_errmsg('','','upd rxe_file',SQLCA.sqlcode,1)
                 LET g_rxe[l_ac7].* = g_rxe_t.*
              ELSE
                 MESSAGE 'UPDATE O.K'
                 COMMIT WORK
              END IF
           END IF

        AFTER ROW
           LET l_ac7 = ARR_CURR()
           LET l_ac7_t = l_ac7

           IF INT_FLAG THEN
              LET INT_FLAG = 0
              IF p_cmd='u' THEN
                 LET g_rxe[l_ac7].* = g_rxe_t.*
              END IF
              CLOSE t600_bcl
              LET g_success = 'N'
              EXIT INPUT
           END IF
           SELECT SUM(rxe08)
             INTO l_rxe08_sum
             FROM rxe_file
            WHERE rxe02 = g_ogb[l_ac].ogb03
              AND rxe00 = '02'
              AND rxe01 = g_oga.oga01
           SELECT COUNT(*) INTO l_n
             FROM ogb_file
            WHERE ogb01 = g_oga.oga01
              AND ogb03 = g_ogb[l_ac].ogb03
           IF l_n > 0 THEN
              UPDATE ogb_file set ogb12 = l_rxe08_sum,
                                  ogb917 = l_rxe08_sum  #FUN-C30169 add
               WHERE ogb01 = g_oga.oga01
                 AND ogb03 = g_ogb[l_ac].ogb03
              LET g_ogb[l_ac].ogb12 = l_rxe08_sum
           ELSE
              LET g_ogb[l_ac].ogb12 = l_rxe08_sum
           END IF
           CLOSE t600_bcl

          ON ACTION CONTROLR
             CALL cl_show_req_fields()

          ON ACTION CONTROLG
             CALL cl_cmdask()

          ON IDLE g_idle_seconds
             CALL cl_on_idle()
             CONTINUE INPUT

          ON ACTION about
             CALL cl_about()

          ON ACTION HELP
             CALL cl_show_help()

