                    WHERE azf01 = g_ogb[l_ac].ogb1001
                      AND azf02 = '2'
                      AND azfacti= 'Y'
               END IF
            END IF
         END IF
      END IF
   END IF  #FUN-CB0087 add
   RETURN TRUE
END FUNCTION

