#中文的MENU
FUNCTION t600_menu()
   DEFINE l_creator   LIKE type_file.chr1        #No.FUN-680137  VARCHAR(1)    #「不准」時是否退回填表人 #FUN-580113
   DEFINE l_flowuser  LIKE type_file.chr1        #No.FUN-680137  VARCHAR(1)    # 是否有指定加簽人員      #FUN-580113
   DEFINE l_oga01     LIKE oga_file.oga01        #MOD-940011
   DEFINE l_str       STRING                     #MOD-940011
   DEFINE l_cnt       LIKE type_file.num5        #MOD-AB0246  add
   DEFINE temp_b      LIKE lpj_file.lpj12        #FUN-BA0069 add
   DEFINE max_lsm05   LIKE lsm_file.lsm05        #FUN-BA0069 add
   DEFINE l_lps_cnt   LIKE type_file.num5        #FUN-C10051 add
   DEFINE l_price1    LIKE ogb_file.ogb14t       #No.FUN-870007
  #FUN-D70075 add str-----
   DEFINE l_ebostep    CHAR(1)  #eB-Online階段碼回傳值
   DEFINE l_eboresp    CHAR(1)  #eB-Online回覆碼回傳值
   DEFINE l_ebocode    CHAR(2)  #eB-Online異動狀態碼
   DEFINE l_ebomsg1    STRING   #eB-Online階段碼中文
   DEFINE l_ebomsg2    STRING   #eB-Online回覆碼中文
   DEFINE l_ebostr     STRING   #訊息
  #FUN-D70075 add end-----
   DEFINE l_oga03     LIKE oga_file.oga03     #CHI-B60094 add
   DEFINE li_result   LIKE type_file.num5     #CHI-B60094 add
   DEFINE l_poz       RECORD LIKE poz_file.*  #CHI-B60094 add
   DEFINE l_oea99     LIKE oea_file.oea99     #CHI-B60094 add
   DEFINE l_flow      LIKE oea_file.oea904    #CHI-B60094 add
   DEFINE l_azp03     LIKE azp_file.azp03     #CHI-B60094 add
   DEFINE l_poy04     LIKE poy_file.poy04     #CHI-B60094 add
   DEFINE l_sql       STRING                  #CHI-B60094 add
   DEFINE p_plant_new LIKE azp_file.azp01     #CHI-B60094 add
   DEFINE l_msg       LIKE type_file.chr1000  #FUN-D50048 add

   LET l_flowuser = "N"   #FUN-580113

   WHILE TRUE
      LET g_chr='@'
      CASE g_b_flag
         WHEN '1'
          CALL t600_bp1("G")
         WHEN '2'
          CALL t600_bp2("G")
         #FUN-CB0014---add---str---
         WHEN 'info_list'
            IF NOT cl_null(g_b_flag) THEN
               CALL t600_list_fill()
            END IF
            CALL t600_bp3("G")
            IF NOT cl_null(g_action_choice) AND l_ac2>0 THEN #將清單的資料回傳到主畫面
               SELECT oga_file.*
                 INTO g_oga.*
                 FROM oga_file
                WHERE oga01=g_oga_l[l_ac2].oga01
            END IF
            IF g_action_choice!= "" THEN
               LET g_b_flag = '1'
               LET l_ac2 = ARR_CURR()
               LET g_jump = l_ac2
               LET mi_no_ask = TRUE
               IF g_rec_b8 >0 THEN
                   CALL t600_fetch('/')
               END IF
               CALL cl_set_comp_visible("page_in", FALSE)
               CALL cl_set_comp_visible("info", FALSE)
               CALL ui.interface.refresh()
               CALL cl_set_comp_visible("page_in", TRUE)
               CALL cl_set_comp_visible("info", TRUE)
             END IF
         #FUN-CB0014---add---end---
         OTHERWISE
          CALL t600_bp1("G")
      END CASE
      CASE g_action_choice
         WHEN "shipment"
            IF cl_chk_act_auth() THEN
               LET g_multi_ima01_flag = '0'   #FUN-D30039 add
               LET g_b3_flag = '0'            #FUN-D70016 add
               CALL t600_b1()
            END IF

         WHEN "return"
            IF cl_chk_act_auth() THEN
               CALL t600_b2()
            END IF

         WHEN "insert"
            IF cl_chk_act_auth() THEN
               CALL t600_a()
            END IF

         WHEN "query"
            IF cl_chk_act_auth() THEN
               CALL t600_q()
            END IF

         WHEN "modify"
            IF cl_chk_act_auth() THEN
               CALL t600_u()
            END IF

         WHEN "delete"
            IF cl_chk_act_auth() THEN
               IF t600_r() THEN
                  CALL t600_after_delete()
               END IF
            END IF

         WHEN "detail"
            IF cl_chk_act_auth() THEN
               IF g_b_flag IS NULL OR g_b_flag ='1' THEN
                  LET g_multi_ima01_flag = '0'   #FUN-D30039 add
                  LET g_b3_flag = '0'            #FUN-D70016 add
                  CALL t600_b1()
               ELSE
                  CALL t600_b2()
               END IF
               CALL t600_show()   #FUN-E60056
               IF g_oga.oga09 ='2' AND g_oga.oga07 = 'Y' THEN
                 #CHI-AC0002 mod --start--
                 #CALL s_t600_gl(g_oga.oga01)
                  CALL s_t600_gl(g_oga.oga01,'0')
                  IF g_aza.aza63 = 'Y' THEN
                     CALL s_t600_gl(g_oga.oga01,'1')
                  END IF
                 #CHI-AC0002 mod --end--
               END IF
            ELSE
               LET g_action_choice = NULL
            END IF

         WHEN "output"
            IF cl_chk_act_auth() THEN
               CALL t600_out()
            END IF
         #FUN-D60031 add-------------str
         WHEN "taste"
            IF cl_chk_act_auth() THEN
               IF NOT cl_null(l_ac) AND l_ac > 0 THEN
                  CALL t600_rxh('Y')
               END IF
            END IF
         WHEN "feed"
            IF cl_chk_act_auth() THEN
               IF NOT cl_null(l_ac) AND l_ac > 0 THEN
                  CALL t600_feed('Y')
               END IF
            END IF
         #FUN-D60031 add-------------end
         #No.FUN-A10106--begin
         WHEN "kefa"
            IF cl_null(g_oga.oga01) THEN
               CALL cl_err('','-400',1)
            ELSE
#FUN-B30012 -------------------STA
#              CALL s_gifts('02',g_oga.oga01,g_oga.ogaplant,g_oga.oga02,g_oga.oga87)
               IF t600_chk_ogb1001_3(g_oga.oga01) THEN
                  CALL s_gifts('02',g_oga.oga01,g_oga.ogaplant,g_oga.oga02,g_oga.oga87)
                  CALL t600_b1_fill(' 1=1')
                  IF g_aza.aza50='Y' THEN
                     CALL t600_oga50_sum()
                  ELSE
                     CALL t600_bu()
                  END IF
               ELSE
                  CALL cl_err('','axm-885',0)
               END IF
