            #IF g_argv0='2' OR g_argv0='4' OR g_argv0='6' OR g_argv0='8' OR g_argv0='9' OR ((g_argv0 ='1' OR g_argv0='5') AND g_oaz.oaz81 = 'Y') THEN   #MOD-940294 add   #CHI-AC0034 add '8','9'   #MOD-AC0060
