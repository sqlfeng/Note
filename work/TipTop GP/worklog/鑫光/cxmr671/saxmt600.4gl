FUNCTION t600_rxe_fill()

    IF g_flag3 = '1' THEN
       LET g_sql = "SELECT rxe02,rxe03,rxe04,rxe05,rxe06,'',rxe07,'',rxe08,rxe09 ",
                   " FROM rxe_file",
                   " WHERE rxe01 = '", g_oga.oga01,"'",
                   "   AND rxe00 = '02' "
    ELSE
       LET g_sql = "SELECT rxe02,rxe03,rxe04,rxe05,rxe06,'',rxe07,'',rxe08,rxe09 ",
                   " FROM rxe_file",
                   " WHERE rxe01 = '", g_oga.oga01,"'",
                   "   AND rxe02 = '", g_ogb[l_ac].ogb03,"'",
                   "   AND rxe00 = '02' "
    END IF
    PREPARE t600_rxe_pb FROM g_sql
    DECLARE t600_rxe_curs CURSOR FOR t600_rxe_pb

    CALL g_rxe.clear()
    LET g_cnt = 1
    MESSAGE "Searching!"
    FOREACH t600_rxe_curs INTO g_rxe[g_cnt].*
       IF STATUS THEN
           LET g_success = 'N'
           CALL s_errmsg('','','FOREACH:',SQLCA.sqlcode,1)
           EXIT FOREACH
       END IF
       SELECT lpx02 INTO g_rxe[g_cnt].lpx02
         FROM lqe_file
   INNER JOIN lpx_file
           ON lqe02 = lpx01
        WHERE lqe01 = g_rxe[g_cnt].rxe04

       SELECT lrz02 INTO g_rxe[g_cnt].lrz02
         FROM lqe_file
   INNER JOIN lrz_file
           ON lqe03 = lrz01
        WHERE lqe01 = g_rxe[g_cnt].rxe04

       LET g_cnt = g_cnt + 1
       IF g_cnt > g_max_rec THEN
       LET g_success = 'N'
           CALL s_errmsg('','','','9035',1)
           EXIT FOREACH
       END IF
    END FOREACH
    CALL g_rxe.deleteElement(g_cnt)
    MESSAGE ""
    LET g_rec_b7 = g_cnt-1
    LET g_cnt = 0
END FUNCTION

FUNCTION t600_rxe04_rxe05(p_no,p_cmd)
 DEFINE p_no        LIKE rxe_file.rxe04
 DEFINE l_cnt       LIKE type_file.num5
 DEFINE p_cmd       LIKE type_file.chr1
 DEFINE l_lqe02     LIKE lqe_file.lqe02
 DEFINE l_lqe03     LIKE lqe_file.lqe03
 DEFINE l_lpx01     LIKE lpx_file.lpx01
 DEFINE l_lpx02     LIKE lpx_file.lpx02
 DEFINE l_lpx22     LIKE lpx_file.lpx22
 DEFINE l_lpx23     LIKE lpx_file.lpx23
 DEFINE l_lpx24     LIKE lpx_file.lpx24
 DEFINE l_rxe07     LIKE rxe_file.rxe07
 DEFINE l_lpx15     LIKE lpx_file.lpx15
 DEFINE l_lpxacti   LIKE lpx_file.lpxacti
 DEFINE l_lrz03     LIKE lrz_file.lrz03
 DEFINE l_lrz02     LIKE lrz_file.lrz02
 DEFINE l_length    LIKE type_file.num5
 DEFINE l_start     LIKE lpz_file.lpz03
 DEFINE l_end       LIKE lpz_file.lpz03
 DEFINE l_lqe17     LIKE lqe_file.lqe17
 DEFINE l_rxe04     LIKE rxe_file.rxe04
 DEFINE l_lqe02_1   LIKE lqe_file.lqe02
 DEFINE l_lqe03_1   LIKE lqe_file.lqe03
 DEFINE l_sql       STRING
 DEFINE l_n         LIKE type_file.num5   #FUN-C30169 add

    LET g_errno = ''
    LET g_success = 'Y'
    SELECT lqe02,lqe03,lqe17 INTO l_lqe02,l_lqe03,l_lqe17
      FROM lqe_file
     WHERE lqe01 = p_no
       AND lqe13 = g_oga.ogaplant
    CASE
         WHEN SQLCA.sqlcode = 100                    LET g_errno = 'alm-477'
         WHEN l_lqe17 <> '5' AND l_lqe17 <> '2'      LET g_errno = 'axm-685'
         OTHERWISE                                   LET g_errno = SQLCA.SQLCODE USING '-------'
    END CASE
    IF NOT cl_null(g_errno) THEN
       RETURN
    END IF
   #FUN-C30169 add START
    SELECT COUNT(*) INTO l_n FROM lpx_file
       WHERE lpx01 = l_lqe02
         AND lpx32 = g_ogb[l_ac].ogb04
    IF l_n = 0 OR cl_null(l_n) THEN
       LET g_errno = 'axm1132'
       LET g_success = 'N'
       RETURN
    END IF
   #FUN-C30169 add END
    #同出貨單+項次的所有資料,券種不可不同,且券面額不可不同
    LET l_sql = " SELECT rxe04 FROM rxe_file",
                "  WHERE rxe01 = '",g_oga.oga01,"'",
                "    AND rxe02 = '",g_rxe[l_ac7].rxe02,"'",
                "    AND rxe00 = '02' "
    PREPARE t600_rxe04_pb FROM l_sql
    DECLARE t600_rxe04_curs CURSOR FOR t600_rxe04_pb
    FOREACH t600_rxe04_curs INTO l_rxe04
       IF STATUS THEN
           LET g_success = 'N'
           CALL s_errmsg('','','FOREACH:',SQLCA.sqlcode,1)
           EXIT FOREACH
       END IF
       SELECT lqe02,lqe03 INTO l_lqe02_1,l_lqe03_1
         FROM lqe_file
        WHERE lqe01 = l_rxe04
       IF l_lqe02_1 <> l_lqe02 THEN
          LET g_errno = 'axm1132'
          LET g_success = 'N'
          EXIT FOREACH
       END IF
       IF l_lqe03_1 <> l_lqe03 THEN
          LET g_errno = 'axm1133'
          LET g_success = 'N'
          EXIT FOREACH
       END IF
    END FOREACH
    IF g_success = 'N' THEN
       RETURN
    END IF
    LET l_length = LENGTH(p_no)
    SELECT lpx01,lpx02,lpx22,lpx23,lpx24,lpx28,lpx15,lpxacti
      INTO l_lpx01,l_lpx02,l_lpx22,l_lpx23,l_lpx24,l_rxe07,l_lpx15,l_lpxacti
      FROM lpx_file
     WHERE lpx01 = l_lqe02
 #   CASE WHEN SQLCA.sqlcode = 100 LET g_errno = 'clm-332'    #FUN-BC0082  mark
     CASE WHEN SQLCA.sqlcode = 100 LET g_errno = 'alm1562'    #FUN-BC0082  add

         WHEN l_lpxacti = 'N'     LET g_errno = '9028'
         WHEN l_lpx15 <> 'Y'      LET g_errno = 'aap-084'
         OTHERWISE                LET g_errno = SQLCA.SQLCODE USING '-------'
    END CASE
    IF cl_null(g_errno) THEN
       IF LENGTH(p_no) - LENGTH(l_lpx23) <> l_lpx24 OR l_lpx23 <> p_no[1,l_lpx22] THEN
          LET g_errno = 'alm-388'
          LET g_success = 'N'
          RETURN
       ELSE
          IF NOT cl_null(l_rxe07) THEN
             SELECT lrz02,lrz03 INTO l_lrz02,l_lrz03
               FROM lrz_file
              WHERE lrz01 = l_rxe07
             CASE WHEN SQLCA.sqlcode = 100 LET g_errno = 'alm-392'
                  WHEN l_lrz03 = 'N'       LET g_errno = '9028'
                  OTHERWISE                LET g_errno = SQLCA.SQLCODE USING '-------'
             END CASE
          ELSE
             LET l_rxe07 = l_lqe03
             SELECT lrz02 INTO l_lrz02 FROM lrz_file
              WHERE lrz01 = l_rxe07
          END IF
       END IF
       IF NOT cl_null(g_errno) THEN
           RETURN
       END IF
       SELECT COUNT(*) INTO l_cnt FROM lnk_file
        WHERE lnk01 = l_lpx01
          AND lnk02 = '2'
          AND lnk03 = g_oga.ogaplant
          AND lnk05 = 'Y'
       IF l_cnt < 1 THEN
          LET g_errno = 'alm-395'
          LET g_success = 'N'
          RETURN
       END IF
       IF p_cmd ='a' OR (g_rxe[l_ac7].rxe05 != g_rxe_t.rxe05
          OR g_rxe[l_ac7].rxe04 != g_rxe_t.rxe04) THEN
          LET l_cnt = 0
          IF p_cmd = 'a' THEN
             SELECT COUNT(*) INTO l_cnt
               FROM rxe_file
              WHERE rxe00 = '02'
                AND rxe01 = g_oga.oga01
                AND rxe04 <= p_no
                AND rxe05 >= p_no
          ELSE
             SELECT COUNT(*) INTO l_cnt
               FROM rxe_file
              WHERE rxe00 = '02'
                AND rxe01 = g_oga.oga01
                AND rxe04 <= p_no
                AND rxe05 >= p_no
                AND rxe03 <> g_rxe[l_ac7].rxe03
          END IF
          IF l_cnt > 0 THEN
             LET g_errno = '-239'
             LET g_success = 'N'
             RETURN
          END IF
       END IF
    END IF
    IF cl_null(g_errno) OR p_cmd = 'd' THEN
       LET g_rxe[l_ac7].rxe06 = l_lpx01
       LET g_rxe[l_ac7].rxe07 = l_rxe07
       LET g_rxe[l_ac7].lpx02 = l_lpx02
       LET g_rxe[l_ac7].lrz02 = l_lrz02
       DISPLAY BY NAME g_rxe[l_ac7].rxe06,g_rxe[l_ac7].rxe07
    END IF
    IF NOT cl_null(g_rxe[l_ac7].rxe05) AND NOT cl_null(g_rxe[l_ac7].rxe04) THEN
       CALL s_showmsg_init()
       CALL t600_rxe04_rxe05_chk(l_lpx01,g_rxe[l_ac7].rxe04,g_rxe[l_ac7].rxe05)
       IF  g_success <> 'Y' THEN
           LET g_success = 'N'
           CALL s_showmsg()
       END IF
       IF g_success = 'Y' THEN
             LET l_start = g_rxe[l_ac7].rxe04[l_lpx22+1,LENGTH(g_rxe[l_ac7].rxe04)]
             LET l_end   = g_rxe[l_ac7].rxe05[l_lpx22+1,LENGTH(g_rxe[l_ac7].rxe05)]
          IF l_end < l_start THEN
             LET g_errno = 'aim-919'
             LET g_success = 'N'
             RETURN
          END IF
       END IF
       IF g_success = 'Y' THEN
          LET g_rxe[l_ac7].rxe08 = l_end - l_start + 1
          LET g_rxe[l_ac7].rxe09 = g_rxe[l_ac7].lrz02 * g_rxe[l_ac7].rxe08
          IF cl_null(g_rxe[l_ac7].rxe09) THEN  #FUN-D10040 add
             LET g_rxe[l_ac7].rxe09 = 0        #FUN-D10040 add
          END IF                               #FUN-D10040 add
          DISPLAY BY NAME g_rxe[l_ac7].rxe08,g_rxe[l_ac7].rxe09
       END IF
    END IF
END FUNCTION

FUNCTION  t600_rxe04_rxe05_chk(l_rxe06,l_rxe04,l_rxe05)
DEFINE l_rxe06   LIKE rxe_file.rxe06
DEFINE l_rxe04   LIKE rxe_file.rxe04
DEFINE l_rxe05   LIKE rxe_file.rxe05
DEFINE l_cnt     LIKE type_file.num5
DEFINE l_lqe01   LIKE lqe_file.lqe01
DEFINE l_lqe02   LIKE lqe_file.lqe02
DEFINE l_lqe02_1 LIKE lqe_file.lqe02
DEFINE l_lqe03   LIKE lqe_file.lqe03
DEFINE l_lqe03_1 LIKE lqe_file.lqe03

   LET g_success = 'Y'

   #檢查券起訖編號是否有不同的券種,是否有面額不同的券
   SELECT lqe02 INTO l_lqe02
     FROM lqe_file
    WHERE lqe01 = l_rxe04
    SELECT lqe02 INTO l_lqe02_1
     FROM lqe_file
    WHERE lqe01 = l_rxe05
   IF l_lqe02 <> l_lqe02_1 THEN
      CALL cl_err(l_rxe04 CLIPPED|| "|" || l_rxe05 CLIPPED,'axm1134',0)
      LET g_success = 'N'
      RETURN
   END IF
   SELECT lqe03 INTO l_lqe03
     FROM lqe_file
    WHERE lqe01 = l_rxe04
    SELECT lqe03 INTO l_lqe03_1
     FROM lqe_file
    WHERE lqe01 = l_rxe05
   IF l_lqe03 <> l_lqe03_1 THEN
      CALL cl_err(l_rxe04 CLIPPED|| "|" || l_rxe05 CLIPPED,'axm1135',0)
      LET g_success = 'N'
      RETURN
   END IF
   #檢查券起訖編號範圍內是否是發放或退回的券
   LET g_sql = "SELECT lqe01 FROM lqe_file ",
               " WHERE lqe01 BETWEEN '",l_rxe04,"' AND '",l_rxe05,"'",
               "   AND (lqe17 <> '5' OR lqe13 <> '",g_oga.ogaplant,"')",
               "   AND (lqe17 <> '2' OR lqe09 <> '",g_oga.ogaplant,"')"
   PREPARE sel_lqe_pre FROM g_sql
   DECLARE sel_lqe_cs CURSOR FOR sel_lqe_pre
   FOREACH sel_lqe_cs INTO l_lqe01
      LET g_success = 'N'
      CALL s_errmsg(l_lqe01,'','','axm-685',1)
      RETURN
   END FOREACH
   #檢查券起訖編號範圍內是否有不同的券種
   SELECT COUNT(DISTINCT lqe02) INTO l_cnt
     FROM lqe_file
    WHERE lqe01 BETWEEN l_rxe04 AND l_rxe05
   IF l_cnt > 1 THEN
      LET g_success = 'N'
      #rxe04值 + "~" + rxe05值 + " 有多種券種，請分開輸入！
      CALL cl_err_msg("","axm-683",l_rxe04 CLIPPED|| "|" || l_rxe05 CLIPPED,10)
      RETURN
   END IF
   #檢查券起訖編號範圍內是否有面額不同的券
   SELECT COUNT(DISTINCT lqe03) INTO l_cnt
     FROM lqe_file
    WHERE lqe01 BETWEEN l_rxe04 AND l_rxe05
   IF l_cnt > 1 THEN
     LET g_success = 'N'
     CALL cl_err_msg("","axm-684",l_rxe04 CLIPPED|| "|" || l_rxe05 CLIPPED,10)
     #rxe04值 + "~" + rxe05值 + " 有多種券面額，請分開輸入！
     RETURN
   END IF
END FUNCTION

