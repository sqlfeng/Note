#檢查客戶簽收單是否已有立AR
FUNCTION t600_chk_ar_exists(p_ogb03)
   DEFINE p_ogb03    LIKE ogb_file.ogb03
   DEFINE l_result   LIKE type_file.num5
   DEFINE l_cnt      LIKE type_file.num5
   DEFINE l_oga01    LIKE oga_file.oga01
   DEFINE l_ogb03    LIKE ogb_file.ogb03

   LET l_result = FALSE

   WHILE TRUE
      LET g_sql = " SELECT DISTINCT oga01,ogb03 ",
                  "   FROM oga_file,ogb_file ",
                  #"  WHERE (oga011 = '",g_oga.oga01,"' OR ",
                  "  WHERE oga01 = ogb01 ",
                  "    AND (ogb66 = '",g_oga.oga01,"' AND ",
                  "         ogb67 =  ",p_ogb03,") ",
                  "    AND oga09 = '8' ",
                  "    AND ogaconf <> 'X' "
      PREPARE t600_sel_oga_p FROM g_sql
      DECLARE t600_sel_oga_c CURSOR FOR t600_sel_oga_p

      FOREACH t600_sel_oga_c INTO l_oga01,l_ogb03
         IF SQLCA.sqlcode THEN
            CALL cl_err('FOREACH t600_sel_oga_c:',SQLCA.sqlcode,1)
            EXIT WHILE
         END IF

         LET l_cnt = 0
         SELECT COUNT(*)
           INTO l_cnt
           FROM omb_file,oma_file
          WHERE omb01 = oma01
            AND omavoid = 'N'
            AND omb31 = l_oga01
            AND omb32 = l_ogb03
         IF l_cnt > 0 THEN
            LET l_result = TRUE
            EXIT WHILE
         END IF
      END FOREACH

      EXIT WHILE
   END WHILE

   RETURN l_result
END FUNCTION

#其他號碼顯示
FUNCTION t600_get_otn(p_ta_ogb01)
   DEFINE p_ta_ogb01   LIKE ogb_file.ta_ogb01
   DEFINE l_azf03a     LIKE azf_file.azf03        #爐別
   DEFINE l_tc_otn04   LIKE tc_otn_file.tc_otn04  #其他號碼
   DEFINE l_tc_otn_str LIKE type_file.chr1000     #其他號碼

   LET l_tc_otn_str = NULL

   IF NOT cl_null(p_ta_ogb01) THEN
      FOREACH t600_sel_otn_c USING p_ta_ogb01
                              INTO l_azf03a,l_tc_otn04
         IF SQLCA.sqlcode THEN
            CALL cl_err('FOREACH t600_sel_otn_c',SQLCA.sqlcode,1)
            EXIT FOREACH
         END IF
         IF cl_null(l_tc_otn_str) THEN
            LET l_tc_otn_str = l_azf03a CLIPPED," ",l_tc_otn04 CLIPPED
         ELSE
            LET l_tc_otn_str = l_tc_otn_str CLIPPED,',',
                               l_azf03a CLIPPED," ",l_tc_otn04 CLIPPED
         END IF
      END FOREACH
   END IF

   RETURN l_tc_otn_str
END FUNCTION

#將其他號碼資料匯入tmp
FUNCTION t600_ins_otn_tmp()
   DEFINE l_ta_ogb01   LIKE ogb_file.ta_ogb01
   DEFINE l_tc_otn_str LIKE type_file.chr1000     #其他號碼
   DEFINE l_sql        STRING

   DELETE FROM t600_otn_tmp
    WHERE 1=1

   BEGIN WORK

   LET l_sql = " SELECT DISTINCT ta_ogb01 ",
               "   FROM ogb_file,oga_file ",
               "  WHERE ogb01 = oga01 ",
               "    AND ta_ogb01 IS NOT NULL ",
               "    AND ",g_wc  CLIPPED,
               "    AND ",g_wc2 CLIPPED,
               "    AND ",g_wc3 CLIPPED,
               "    AND EXISTS(SELECT 1 ",
               "                 FROM inb_file,tc_otn_file ",
               "                WHERE inb01 = tc_otn01 ",
               "                  AND ta_inb04 = ta_ogb01) "
   PREPARE t600_sel_ogab_p FROM l_sql
   DECLARE t600_sel_ogab_c CURSOR FOR t600_sel_ogab_p

   LET l_sql = "INSERT INTO t600_otn_tmp VALUES(?,?) "
   PREPARE t600_ins_otn_p FROM l_sql
   DECLARE t600_ins_otn_c CURSOR FOR t600_ins_otn_p

   OPEN t600_ins_otn_c

   FOREACH t600_sel_ogab_c INTO l_ta_ogb01
      IF SQLCA.sqlcode THEN
         CALL cl_err('FOREACH t600_sel_ogab_c:',SQLCA.sqlcode,1)
         EXIT FOREACH
      END IF

      CALL t600_get_otn(l_ta_ogb01)
         RETURNING l_tc_otn_str

      PUT t600_ins_otn_c FROM l_ta_ogb01,l_tc_otn_str
   END FOREACH

   FLUSH t600_ins_otn_c
   IF SQLCA.sqlcode THEN
      CALL cl_err('ins t600_otn_tmp:',SQLCA.sqlcode,1)
   END IF

   CLOSE t600_ins_otn_c

   COMMIT WORK
END FUNCTION
