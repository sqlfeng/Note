   END IF
   IF NOT cl_null(g_ogb[l_ac].ogb916) THEN
      SELECT gfe02 INTO g_buf FROM gfe_file
       WHERE gfe01=g_ogb[l_ac].ogb916
         AND gfeacti='Y'
      IF STATUS THEN
         CALL cl_err3("sel","gfe_file",g_ogb[l_ac].ogb916,"",SQLCA.sqlcode,"","gfe:",1)  #No.FUN-670008
         RETURN "ogb916"
