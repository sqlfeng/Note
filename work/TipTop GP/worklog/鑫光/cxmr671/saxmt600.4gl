            IF SQLCA.sqlcode THEN
               CALL cl_err3("ins","oao_file",g_oga.oga01,"",SQLCA.sqlcode,"","ins oao",1)
               ROLLBACK WORK
               EXIT FOREACH
            END IF
         END FOREACH
         #CHI-E30016 add end----------------------------------------------------
      END FOREACH
   END IF
